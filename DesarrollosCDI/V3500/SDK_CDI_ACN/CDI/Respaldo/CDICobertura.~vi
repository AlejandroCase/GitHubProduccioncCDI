[Vista.ListaCampos]
(Inicio)=CDICobertura.Id
CDICobertura.Id=CDICobertura.Clave
CDICobertura.Clave=CDICobertura.Descripcion
CDICobertura.Descripcion=CDICobertura.Importe
CDICobertura.Importe=CDICobertura.Porcentaje
CDICobertura.Porcentaje=CDICobertura.Articulo
CDICobertura.Articulo=CDICobertura.Periodo
CDICobertura.Periodo=CDICobertura.NoPeriodo
CDICobertura.NoPeriodo=CDICobertura.MesInicial
CDICobertura.MesInicial=CDICobertura.MesFinal
CDICobertura.MesFinal=CDICobertura.FechaInicio
CDICobertura.FechaInicio=CDICobertura.FechaFin
CDICobertura.FechaFin=CDICobertura.Estatus
CDICobertura.Estatus=CDICobertura.MesInicialN
CDICobertura.MesInicialN=CDICobertura.MesFinalN
CDICobertura.MesFinalN=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDICobertura.Id,
CDICobertura.Id,=CDICobertura.Clave,
CDICobertura.Clave,=CDICobertura.Descripcion,
CDICobertura.Descripcion,=CDICobertura.Importe,
CDICobertura.Importe,=CDICobertura.Porcentaje,
CDICobertura.Porcentaje,=CDICobertura.Articulo,
CDICobertura.Articulo,=CDICobertura.Periodo,
CDICobertura.Periodo,=CDICobertura.NoPeriodo,
CDICobertura.NoPeriodo,=CDICobertura.MesInicial,
CDICobertura.MesInicial,=CDICobertura.MesFinal,
CDICobertura.MesFinal,=CDICobertura.FechaInicio,
CDICobertura.FechaInicio,=CDICobertura.FechaFin,
CDICobertura.FechaFin,=CDICobertura.Estatus,
CDICobertura.Estatus,=CDICobertura.MesInicialN,
CDICobertura.MesInicialN,=CDICobertura.MesFinalN
CDICobertura.MesFinalN=FROM CDICobertura
FROM CDICobertura=(Fin)
