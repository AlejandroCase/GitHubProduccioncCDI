[Tabla.ListaCampos]
(Inicio)=Id
Id=Mov
Mov=FechaEmision
FechaEmision=Personal
Personal=HoraRegistro
HoraRegistro=PersonalIntenta
PersonalIntenta=MsgError
MsgError=Origen
Origen=Biometrico
Biometrico=Ejercicio
Ejercicio=Periodo
Periodo=Renglon
Renglon=RegistroAsis
RegistroAsis=FechaRegistroAsis
FechaRegistroAsis=usuario
usuario=MovAsis
MovAsis=MovIdAsis
MovIdAsis=(Fin)

[Tabla.SQL]
(Inicio)=SELECT
SELECT=Id,
Id,=Mov,
Mov,=FechaEmision,
FechaEmision,=Ejercicio,
Ejercicio,=Periodo,
Periodo,=Personal,
Personal,=HoraRegistro,
HoraRegistro,=PersonalIntenta,
PersonalIntenta,=MsgError,
MsgError,=Origen,
Origen,=Biometrico,
Biometrico,=Renglon,
Renglon,=RegistroAsis,
RegistroAsis,=FechaRegistroAsis,
FechaRegistroAsis,=usuario,
usuario,=MovAsis,
MovAsis,=MovIdAsis
MovIdAsis=FROM CDIVAsisteLog
FROM CDIVAsisteLog=(Fin)

[Tabla.CamposBusquedaRapida]
(Inicio)=Personal
Personal=PersonalIntenta
PersonalIntenta=(Fin)
