[Tabla.ListaCampos]
(Inicio)=mov
mov=movid
movid=fechaemision
fechaemision=estatus
estatus=observaciones
observaciones=ejercicio
ejercicio=periodo
periodo=renglon
renglon=personal
personal=cantidad
cantidad=referencia
referencia=fechad
fechad=fechaa
fechaa=cdidiasdescanso
cdidiasdescanso=CDIDiasFestivo
CDIDiasFestivo=Empleado
Empleado=(Fin)

[Tabla.SQL]
(Inicio)=SELECT
SELECT=mov,
mov,=movid,
movid,=fechaemision,
fechaemision,=estatus,
estatus,=observaciones,
observaciones,=ejercicio,
ejercicio,=periodo,
periodo,=renglon,
renglon,=personal,
personal,=Empleado,
Empleado,=cantidad,
cantidad,=referencia,
referencia,=fechad,
fechad,=fechaa,
fechaa,=cdidiasdescanso,
cdidiasdescanso,=CDIDiasFestivo
CDIDiasFestivo=FROM CDIDetalleVaca
FROM CDIDetalleVaca=(Fin)

