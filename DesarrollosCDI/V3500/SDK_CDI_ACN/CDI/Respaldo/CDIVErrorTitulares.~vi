[Vista.ListaCampos]
(Inicio)=CDIVErrorTitulares.Tipo
CDIVErrorTitulares.Tipo=CDIVErrorTitulares.Categoria
CDIVErrorTitulares.Categoria=CDIVErrorTitulares.SubCategoria
CDIVErrorTitulares.SubCategoria=CDIVErrorTitulares.Aporta
CDIVErrorTitulares.Aporta=CDIVErrorTitulares.Clase
CDIVErrorTitulares.Clase=CDIVErrorTitulares.Actividad
CDIVErrorTitulares.Actividad=CDIVErrorTitulares.Sede
CDIVErrorTitulares.Sede=CDIVErrorTitulares.Titular
CDIVErrorTitulares.Titular=CDIVErrorTitulares.Estatus
CDIVErrorTitulares.Estatus=CDIVErrorTitulares.Cliente
CDIVErrorTitulares.Cliente=CDIVErrorTitulares.Membresia
CDIVErrorTitulares.Membresia=CDIVErrorTitulares.Socio
CDIVErrorTitulares.Socio=CDIVErrorTitulares.Nombre
CDIVErrorTitulares.Nombre=CDIVErrorTitulares.Email1
CDIVErrorTitulares.Email1=CDIVErrorTitulares.Email2
CDIVErrorTitulares.Email2=CDIVErrorTitulares.CDISubsidio
CDIVErrorTitulares.CDISubsidio=CDIVErrorTitulares.Descuento
CDIVErrorTitulares.Descuento=CDIVErrorTitulares.CDICR
CDIVErrorTitulares.CDICR=CDIVErrorTitulares.CDIFacturarA
CDIVErrorTitulares.CDIFacturarA=CDIVErrorTitulares.Edad
CDIVErrorTitulares.Edad=CDIVErrorTitulares.FechaNacimiento
CDIVErrorTitulares.FechaNacimiento=CDIVErrorTitulares.DefCargAuto
CDIVErrorTitulares.DefCargAuto=CDIVErrorTitulares.Articulo
CDIVErrorTitulares.Articulo=CDIVErrorTitulares.ArticuloCR
CDIVErrorTitulares.ArticuloCR=CDIVErrorTitulares.Descripcion1
CDIVErrorTitulares.Descripcion1=CDIVErrorTitulares.CDIFormapago
CDIVErrorTitulares.CDIFormapago=CDIVErrorTitulares.PrecioLista
CDIVErrorTitulares.PrecioLista=CDIVErrorTitulares.integrantes
CDIVErrorTitulares.integrantes=CDIVErrorTitulares.ErrorTitulares
CDIVErrorTitulares.ErrorTitulares=CDIVErrorTitulares.ErrorSubsidioNegativo
CDIVErrorTitulares.ErrorSubsidioNegativo=CDIVErrorTitulares.ErrorCategoria
CDIVErrorTitulares.ErrorCategoria=CDIVErrorTitulares.ErrorTipoSubsPrecio
CDIVErrorTitulares.ErrorTipoSubsPrecio=CDIVErrorTitulares.ErrorSubsMayorPrecio
CDIVErrorTitulares.ErrorSubsMayorPrecio=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDIVErrorTitulares.Tipo,
CDIVErrorTitulares.Tipo,=CDIVErrorTitulares.Categoria,
CDIVErrorTitulares.Categoria,=CDIVErrorTitulares.SubCategoria,
CDIVErrorTitulares.SubCategoria,=CDIVErrorTitulares.Aporta,
CDIVErrorTitulares.Aporta,=CDIVErrorTitulares.Clase,
CDIVErrorTitulares.Clase,=CDIVErrorTitulares.Actividad,
CDIVErrorTitulares.Actividad,=CDIVErrorTitulares.Sede,
CDIVErrorTitulares.Sede,=CDIVErrorTitulares.Titular,
CDIVErrorTitulares.Titular,=CDIVErrorTitulares.Estatus,
CDIVErrorTitulares.Estatus,=CDIVErrorTitulares.Cliente,
CDIVErrorTitulares.Cliente,=CDIVErrorTitulares.Membresia,
CDIVErrorTitulares.Membresia,=CDIVErrorTitulares.Socio,
CDIVErrorTitulares.Socio,=CDIVErrorTitulares.Nombre,
CDIVErrorTitulares.Nombre,=CDIVErrorTitulares.Email1,
CDIVErrorTitulares.Email1,=CDIVErrorTitulares.Email2,
CDIVErrorTitulares.Email2,=CDIVErrorTitulares.CDISubsidio,
CDIVErrorTitulares.CDISubsidio,=CDIVErrorTitulares.Descuento,
CDIVErrorTitulares.Descuento,=CDIVErrorTitulares.CDICR,
CDIVErrorTitulares.CDICR,=CDIVErrorTitulares.CDIFacturarA,
CDIVErrorTitulares.CDIFacturarA,=CDIVErrorTitulares.Edad,
CDIVErrorTitulares.Edad,=CDIVErrorTitulares.FechaNacimiento,
CDIVErrorTitulares.FechaNacimiento,=CDIVErrorTitulares.DefCargAuto,
CDIVErrorTitulares.DefCargAuto,=CDIVErrorTitulares.Articulo,
CDIVErrorTitulares.Articulo,=CDIVErrorTitulares.ArticuloCR,
CDIVErrorTitulares.ArticuloCR,=CDIVErrorTitulares.Descripcion1,
CDIVErrorTitulares.Descripcion1,=CDIVErrorTitulares.CDIFormapago,
CDIVErrorTitulares.CDIFormapago,=CDIVErrorTitulares.PrecioLista,
CDIVErrorTitulares.PrecioLista,=CDIVErrorTitulares.integrantes,
CDIVErrorTitulares.integrantes,=CDIVErrorTitulares.ErrorTitulares,
CDIVErrorTitulares.ErrorTitulares,=CDIVErrorTitulares.ErrorSubsidioNegativo,
CDIVErrorTitulares.ErrorSubsidioNegativo,=CDIVErrorTitulares.ErrorCategoria,
CDIVErrorTitulares.ErrorCategoria,=CDIVErrorTitulares.ErrorTipoSubsPrecio,
CDIVErrorTitulares.ErrorTipoSubsPrecio,=CDIVErrorTitulares.ErrorSubsMayorPrecio
CDIVErrorTitulares.ErrorSubsMayorPrecio=FROM CDIVErrorTitulares
FROM CDIVErrorTitulares=(Fin)
