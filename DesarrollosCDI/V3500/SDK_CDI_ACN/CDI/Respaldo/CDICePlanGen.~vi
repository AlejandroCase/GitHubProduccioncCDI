[Vista.SQL]
(Inicio)=SELECT
SELECT=CDICePlanGen.Ceplan,
CDICePlanGen.Ceplan,=CDICePlanGen.Descripcion,
CDICePlanGen.Descripcion,=CDICePlanGen.Nivel,
CDICePlanGen.Nivel,=CDICePlanGen.CreditoDirecto,
CDICePlanGen.CreditoDirecto,=CDICePlanGen.Creditos,
CDICePlanGen.Creditos,=CDICePlanGen.CreditosOpcional,
CDICePlanGen.CreditosOpcional,=CDICePlanGen.Inscripcion,
CDICePlanGen.Inscripcion,=CDICePlanGen.Precio,
CDICePlanGen.Precio,=CDICePlanGen.MonedaPrecio,
CDICePlanGen.MonedaPrecio,=CDICePlanGen.Impuesto1,
CDICePlanGen.Impuesto1,=CDICePlanGen.Condicion,
CDICePlanGen.Condicion,=CDICePlanGen.Estatus,
CDICePlanGen.Estatus,=CDICePlanGen.HorarioVariable,
CDICePlanGen.HorarioVariable,=CDICePlanGen.Coordinacion,
CDICePlanGen.Coordinacion,=CDICePlanGen.Evaluacion,
CDICePlanGen.Evaluacion,=CDICePlanGen.Aprobacion,
CDICePlanGen.Aprobacion,=CDICePlanGen.CDIPermiteBeca,
CDICePlanGen.CDIPermiteBeca,=CDICePlanGen.CDIConcepto,
CDICePlanGen.CDIConcepto,=CDICePlanGen.CDIComite,
CDICePlanGen.CDIComite,=CDICePlanGen.CDIEdadMinima,
CDICePlanGen.CDIEdadMinima,=CDICePlanGen.CDIEdadMaxima,
CDICePlanGen.CDIEdadMaxima,=CDICePlanGen.CDIEstaturaMinima,
CDICePlanGen.CDIEstaturaMinima,=CDICePlanGen.CDIPesoMinimo,
CDICePlanGen.CDIPesoMinimo,=CDICePlanGen.CDISexo,
CDICePlanGen.CDISexo,=CDICePlanGen.CDICoberturaMedica,
CDICePlanGen.CDICoberturaMedica,=CDICePlanGen.CDIEvaluacionProfesor,
CDICePlanGen.CDIEvaluacionProfesor,=CDICePlanGen.CDIEvaluacionPsicologica,
CDICePlanGen.CDIEvaluacionPsicologica,=CDICePlanGen.CDICondicionFisica,
CDICePlanGen.CDICondicionFisica,=CDICePlanGen.CDIDatosMedicos,
CDICePlanGen.CDIDatosMedicos,=CDICePlanGen.CDIDatosTransporte,
CDICePlanGen.CDIDatosTransporte,=CDICePlanGen.CDIMostrarInternet,
CDICePlanGen.CDIMostrarInternet,=CDICePlanGen.CDIDiasVigenciaPreRegistro,
CDICePlanGen.CDIDiasVigenciaPreRegistro,=CDICePlanGen.CDILimiteMesesDatosMedicos,
CDICePlanGen.CDILimiteMesesDatosMedicos,=CDICePlanGen.CDICentroCostos,
CDICePlanGen.CDICentroCostos,=CDICePlanGen.CDIClavePresupuestal,
CDICePlanGen.CDIClavePresupuestal,=CDICePlanGen.CDIPeriodo,
CDICePlanGen.CDIPeriodo,=CDICePlanGen.CDICobertura,
CDICePlanGen.CDICobertura,=CDICePlanGen.CDIPeriodoPreciosYdescuentos,
CDICePlanGen.CDIPeriodoPreciosYdescuentos,=CDICePlanGen.CDIGeneraCargoRecurrente,
CDICePlanGen.CDIGeneraCargoRecurrente,=CDICePlanGen.Id
CDICePlanGen.Id=FROM CDICePlanGen
FROM CDICePlanGen=(Fin)

