[Vista.ListaCampos]
(Inicio)=CDICorreoWeb.Id
CDICorreoWeb.Id=CDICorreoWeb.IdCorreo
CDICorreoWeb.IdCorreo=CDICorreoWeb.Modulo
CDICorreoWeb.Modulo=CDICorreoWeb.Titulo
CDICorreoWeb.Titulo=CDICorreoWeb.Descripcion
CDICorreoWeb.Descripcion=CDICorreoWeb.Para
CDICorreoWeb.Para=CDICorreoWeb.Cc
CDICorreoWeb.Cc=CDICorreoWeb.Cco
CDICorreoWeb.Cco=CDICorreoWeb.Estatus
CDICorreoWeb.Estatus=CDICorreoWeb.Asunto
CDICorreoWeb.Asunto=CDICorreoWeb.Saludo
CDICorreoWeb.Saludo=CDICorreoWeb.Cuerpo
CDICorreoWeb.Cuerpo=CDICorreoWeb.Despedida
CDICorreoWeb.Despedida=CDICorreoWeb.EnviarSocio
CDICorreoWeb.EnviarSocio=CDICorreoWeb.EnviarPerfilInterno
CDICorreoWeb.EnviarPerfilInterno=CDICorreoWeb.EnviarSocioPerfil
CDICorreoWeb.EnviarSocioPerfil=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDICorreoWeb.Id,
CDICorreoWeb.Id,=CDICorreoWeb.IdCorreo,
CDICorreoWeb.IdCorreo,=CDICorreoWeb.Modulo,
CDICorreoWeb.Modulo,=CDICorreoWeb.Titulo,
CDICorreoWeb.Titulo,=CDICorreoWeb.Descripcion,
CDICorreoWeb.Descripcion,=CDICorreoWeb.Para,
CDICorreoWeb.Para,=CDICorreoWeb.Cc,
CDICorreoWeb.Cc,=CDICorreoWeb.Cco,
CDICorreoWeb.Cco,=CDICorreoWeb.Estatus,
CDICorreoWeb.Estatus,=CDICorreoWeb.Asunto,
CDICorreoWeb.Asunto,=CDICorreoWeb.Saludo,
CDICorreoWeb.Saludo,=CDICorreoWeb.Cuerpo,
CDICorreoWeb.Cuerpo,=CDICorreoWeb.Despedida,
CDICorreoWeb.Despedida,=CDICorreoWeb.EnviarSocio,
CDICorreoWeb.EnviarSocio,=CDICorreoWeb.EnviarPerfilInterno,
CDICorreoWeb.EnviarPerfilInterno,=CDICorreoWeb.EnviarSocioPerfil
CDICorreoWeb.EnviarSocioPerfil=FROM CDICorreoWeb
FROM CDICorreoWeb=(Fin)
