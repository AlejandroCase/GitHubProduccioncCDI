[CDIBoniCompras.ListaEnCaptura]
(Inicio)=CDIBoniCompras.Mov
CDIBoniCompras.Mov=CDIBoniCompras.Movid
CDIBoniCompras.Movid=CDIBoniCompras.FechaEmision
CDIBoniCompras.FechaEmision=CDIBoniCompras.Articulo
CDIBoniCompras.Articulo=Art.Descripcion1
Art.Descripcion1=Art.Unidad
Art.Unidad=CDIBoniCompras.Cantidad
CDIBoniCompras.Cantidad=CDIBoniCompras.Costo
CDIBoniCompras.Costo=Importe
Importe=CDIBoniCompras.ClavePresupuestal
CDIBoniCompras.ClavePresupuestal=CDIBoniCompras.CDICantBonif
CDIBoniCompras.CDICantBonif=CDIBoniCompras.CDICostBonif
CDIBoniCompras.CDICostBonif=CDIBoniCompras.Observaciones
CDIBoniCompras.Observaciones=(Fin)

[CDIBoniCompras.ListaCamposAValidar]
(Inicio)=Prov.Proveedor
Prov.Proveedor=Prov.Nombre
Prov.Nombre=CDIBoniCompras.MovOC
CDIBoniCompras.MovOC=CDIBoniCompras.MovIDOc
CDIBoniCompras.MovIDOc=(Fin)

