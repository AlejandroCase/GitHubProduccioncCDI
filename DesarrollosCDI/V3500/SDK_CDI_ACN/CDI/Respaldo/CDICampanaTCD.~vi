[Vista.ListaCampos]
(Inicio)=CDICampanaTCD.ID
CDICampanaTCD.ID=CDICampanaTCD.Amex
CDICampanaTCD.Amex=CDICampanaTCD.Bancaria
CDICampanaTCD.Bancaria=CDICampanaTCD.TipoTarjeta
CDICampanaTCD.TipoTarjeta=CDICampanaTCD.Tc1
CDICampanaTCD.Tc1=CDICampanaTCD.Cs1
CDICampanaTCD.Cs1=CDICampanaTCD.Tc2
CDICampanaTCD.Tc2=CDICampanaTCD.Cs2
CDICampanaTCD.Cs2=CDICampanaTCD.FechaVencimiento
CDICampanaTCD.FechaVencimiento=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDICampanaTCD.ID,
CDICampanaTCD.ID,=CDICampanaTCD.Amex,
CDICampanaTCD.Amex,=CDICampanaTCD.Bancaria,
CDICampanaTCD.Bancaria,=CDICampanaTCD.TipoTarjeta,
CDICampanaTCD.TipoTarjeta,=CDICampanaTCD.Tc1,
CDICampanaTCD.Tc1,=CDICampanaTCD.Cs1,
CDICampanaTCD.Cs1,=CDICampanaTCD.Tc2,
CDICampanaTCD.Tc2,=CDICampanaTCD.Cs2,
CDICampanaTCD.Cs2,=CDICampanaTCD.FechaVencimiento
CDICampanaTCD.FechaVencimiento=FROM CDICampanaTCD
FROM CDICampanaTCD=(Fin)
