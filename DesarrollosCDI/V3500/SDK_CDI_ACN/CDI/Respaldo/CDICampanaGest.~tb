[Tabla.ListaCampos]
(Inicio)=Id
Id=RneglonId
RneglonId=Accion
Accion=Acuerdo
Acuerdo=Contacto
Contacto=FechaCompromiso
FechaCompromiso=FormaPago
FormaPago=Tel
Tel=Telefono
Telefono=Cel
Cel=Celular
Celular=TelOf
TelOf=TelOfna
TelOfna=Dir
Dir=Direccion
Direccion=Cor
Cor=Correo
Correo=DireccionNumero
DireccionNumero=DireccionNumeroInt
DireccionNumeroInt=EntreCalles
EntreCalles=Delegacion
Delegacion=Colonia
Colonia=Poblacion
Poblacion=Estado
Estado=Pais
Pais=CodigoPostal
CodigoPostal=AccionPersona
AccionPersona=AcuerdoPersona
AcuerdoPersona=ContactoPersona
ContactoPersona=(Fin)

[Tabla.SQL]
(Inicio)=SELECT
SELECT=Id,
Id,=RneglonId,
RneglonId,=Accion,
Accion,=Acuerdo,
Acuerdo,=Contacto,
Contacto,=FechaCompromiso,
FechaCompromiso,=FormaPago,
FormaPago,=Tel,
Tel,=Telefono,
Telefono,=Cel,
Cel,=Celular,
Celular,=TelOf,
TelOf,=TelOfna,
TelOfna,=Dir,
Dir,=Cor,
Cor,=Correo,
Correo,=Direccion,
Direccion,=DireccionNumero,
DireccionNumero,=DireccionNumeroInt,
DireccionNumeroInt,=EntreCalles,
EntreCalles,=Delegacion,
Delegacion,=Colonia,
Colonia,=Poblacion,
Poblacion,=Estado,
Estado,=Pais,
Pais,=CodigoPostal,
CodigoPostal,=AccionPersona,
AccionPersona,=AcuerdoPersona,
AcuerdoPersona,=ContactoPersona
ContactoPersona=FROM CDICampanaGest
FROM CDICampanaGest=(Fin)
