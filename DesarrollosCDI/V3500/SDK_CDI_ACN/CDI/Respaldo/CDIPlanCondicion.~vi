[Vista.ListaCampos]
(Inicio)=CDIPlanCondicion.Id
CDIPlanCondicion.Id=CDIPlanCondicion.Condicion
CDIPlanCondicion.Condicion=CDIPlanCondicion.TieneMovimientos
CDIPlanCondicion.TieneMovimientos=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDIPlanCondicion.Id,
CDIPlanCondicion.Id,=CDIPlanCondicion.Condicion,
CDIPlanCondicion.Condicion,=CDIPlanCondicion.TieneMovimientos
CDIPlanCondicion.TieneMovimientos=FROM CDIPlanCondicion
FROM CDIPlanCondicion=(Fin)
