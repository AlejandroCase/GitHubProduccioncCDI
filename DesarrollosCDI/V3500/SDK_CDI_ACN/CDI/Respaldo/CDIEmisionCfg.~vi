[Vista.ListaCampos]
(Inicio)=CDIEmisionCfg.Id
CDIEmisionCfg.Id=CDIEmisionCfg.Empresa
CDIEmisionCfg.Empresa=CDIEmisionCfg.Clave
CDIEmisionCfg.Clave=CDIEmisionCfg.Descripcion
CDIEmisionCfg.Descripcion=CDIEmisionCfg.Concepto
CDIEmisionCfg.Concepto=CDIEmisionCfg.Documento
CDIEmisionCfg.Documento=CDIEmisionCfg.CvePresupuestal
CDIEmisionCfg.CvePresupuestal=CDIEmisionCfg.FormaPago
CDIEmisionCfg.FormaPago=CDIEmisionCfg.Dia
CDIEmisionCfg.Dia=CDIEmisionCfg.PeriodoVencimiento
CDIEmisionCfg.PeriodoVencimiento=CDIEmisionCfg.DiaVencimiento
CDIEmisionCfg.DiaVencimiento=CDIEmisionCfg.Principal
CDIEmisionCfg.Principal=CDIEmisionCfg.Estatus
CDIEmisionCfg.Estatus=CDIEmisionCfg.Usuario
CDIEmisionCfg.Usuario=CDIEmisionCfg.FechaRegistro
CDIEmisionCfg.FechaRegistro=CDIEmisionCfg.ControlRenglon
CDIEmisionCfg.ControlRenglon=CDIEmisionCfg.ProgUnica
CDIEmisionCfg.ProgUnica=CDIEmisionCfg.ProgMes
CDIEmisionCfg.ProgMes=CDIEmisionCfg.PorgAnual
CDIEmisionCfg.PorgAnual=CDIEmisionCfg.TipoEmision
CDIEmisionCfg.TipoEmision=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDIEmisionCfg.Id,
CDIEmisionCfg.Id,=CDIEmisionCfg.Empresa,
CDIEmisionCfg.Empresa,=CDIEmisionCfg.Clave,
CDIEmisionCfg.Clave,=CDIEmisionCfg.Descripcion,
CDIEmisionCfg.Descripcion,=CDIEmisionCfg.Concepto,
CDIEmisionCfg.Concepto,=CDIEmisionCfg.Documento,
CDIEmisionCfg.Documento,=CDIEmisionCfg.CvePresupuestal,
CDIEmisionCfg.CvePresupuestal,=CDIEmisionCfg.FormaPago,
CDIEmisionCfg.FormaPago,=CDIEmisionCfg.Dia,
CDIEmisionCfg.Dia,=CDIEmisionCfg.PeriodoVencimiento,
CDIEmisionCfg.PeriodoVencimiento,=CDIEmisionCfg.DiaVencimiento,
CDIEmisionCfg.DiaVencimiento,=CDIEmisionCfg.Principal,
CDIEmisionCfg.Principal,=CDIEmisionCfg.Estatus,
CDIEmisionCfg.Estatus,=CDIEmisionCfg.Usuario,
CDIEmisionCfg.Usuario,=CDIEmisionCfg.FechaRegistro,
CDIEmisionCfg.FechaRegistro,=CDIEmisionCfg.ControlRenglon,
CDIEmisionCfg.ControlRenglon,=CDIEmisionCfg.ProgUnica,
CDIEmisionCfg.ProgUnica,=CDIEmisionCfg.ProgMes,
CDIEmisionCfg.ProgMes,=CDIEmisionCfg.PorgAnual,
CDIEmisionCfg.PorgAnual,=CDIEmisionCfg.TipoEmision
CDIEmisionCfg.TipoEmision=FROM CDIEmisionCfg
FROM CDIEmisionCfg=(Fin)
