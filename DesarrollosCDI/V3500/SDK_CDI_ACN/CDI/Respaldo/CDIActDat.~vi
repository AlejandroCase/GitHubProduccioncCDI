[Vista.SQL]
(Inicio)=SELECT
SELECT=CDIActDat.membresia,
CDIActDat.membresia,=CDIActDat.Cliente,
CDIActDat.Cliente,=CDIActDat.Socio,
CDIActDat.Socio,=CDIActDat.Nombre,
CDIActDat.Nombre,=CDIActDat.PersonalApellidoPaterno,
CDIActDat.PersonalApellidoPaterno,=CDIActDat.PersonalApellidoMaterno,
CDIActDat.PersonalApellidoMaterno,=CDIActDat.PersonalNombres,
CDIActDat.PersonalNombres,=CDIActDat.Sexo,
CDIActDat.Sexo,=CDIActDat.Nacionalidad,
CDIActDat.Nacionalidad,=CDIActDat.FechaNacimiento,
CDIActDat.FechaNacimiento,=CDIActDat.EstadoCivil,
CDIActDat.EstadoCivil,=CDIActDat.Ocupacion,
CDIActDat.Ocupacion,=CDIActDat.Direccion,
CDIActDat.Direccion,=CDIActDat.DireccionNumero,
CDIActDat.DireccionNumero,=CDIActDat.DireccionNumeroInt,
CDIActDat.DireccionNumeroInt,=CDIActDat.EntreCalles,
CDIActDat.EntreCalles,=CDIActDat.Colonia,
CDIActDat.Colonia,=CDIActDat.Delegacion,
CDIActDat.Delegacion,=CDIActDat.Estado,
CDIActDat.Estado,=CDIActDat.Pais,
CDIActDat.Pais,=CDIActDat.CodigoPostal,
CDIActDat.CodigoPostal,=CDIActDat.RFC,
CDIActDat.RFC,=CDIActDat.Telefonos,
CDIActDat.Telefonos,=CDIActDat.TelefonosLada,
CDIActDat.TelefonosLada,=CDIActDat.eMail1,
CDIActDat.eMail1,=CDIActDat.Descripcion19,
CDIActDat.Descripcion19,=CDIActDat.CDIMedTipoSangre,
CDIActDat.CDIMedTipoSangre,=CDIActDat.CDIMedDonador,
CDIActDat.CDIMedDonador,=CDIActDat.CDIMedEnfermo,
CDIActDat.CDIMedEnfermo,=CDIActDat.CDIMedAlergias,
CDIActDat.CDIMedAlergias,=CDIActDat.cdimedmedicamento,
CDIActDat.cdimedmedicamento,=CDIActDat.CDIMedAvisar,
CDIActDat.CDIMedAvisar,=CDIActDat.cdimedavispare1,
CDIActDat.cdimedavispare1,=CDIActDat.cdimedavispare1Telfijo1,
CDIActDat.cdimedavispare1Telfijo1,=CDIActDat.cdimedavispare1TelMov1,
CDIActDat.cdimedavispare1TelMov1,=CDIActDat.CDIMedAvisar2,
CDIActDat.CDIMedAvisar2,=CDIActDat.cdimedavispare2,
CDIActDat.cdimedavispare2,=CDIActDat.cdimedavispare1Telfijo2,
CDIActDat.cdimedavispare1Telfijo2,=CDIActDat.cdimedavispare1TelMov2
CDIActDat.cdimedavispare1TelMov2=FROM CDIActDat
FROM CDIActDat=(Fin)

[Vista.CamposBusquedaRapida]
(Inicio)=CDIActDat.Cliente
CDIActDat.Cliente=CDIActDat.Socio
CDIActDat.Socio=CDIActDat.membresia
CDIActDat.membresia=(Fin)
