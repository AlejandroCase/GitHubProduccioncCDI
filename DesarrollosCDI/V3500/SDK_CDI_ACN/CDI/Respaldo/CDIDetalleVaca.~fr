[CDIDetalleVaca.ListaEnCaptura]
(Inicio)=CDIDetalleVaca.mov
CDIDetalleVaca.mov=CDIDetalleVaca.movid
CDIDetalleVaca.movid=CDIDetalleVaca.fechaemision
CDIDetalleVaca.fechaemision=CDIDetalleVaca.observaciones
CDIDetalleVaca.observaciones=CDIDetalleVaca.referencia
CDIDetalleVaca.referencia=CDIDetalleVaca.estatus
CDIDetalleVaca.estatus=CDIDetalleVaca.cantidad
CDIDetalleVaca.cantidad=CDIDetalleVaca.fechad
CDIDetalleVaca.fechad=CDIDetalleVaca.fechaa
CDIDetalleVaca.fechaa=(Fin)

[CDIDetalleVaca.ListaCamposAValidar]
