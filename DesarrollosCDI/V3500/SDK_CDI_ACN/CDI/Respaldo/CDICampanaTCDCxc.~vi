[Vista.ListaCampos]
(Inicio)=CDICampanaTCDCxc.ID
CDICampanaTCDCxc.ID=CDICampanaTCDCxc.Amex
CDICampanaTCDCxc.Amex=CDICampanaTCDCxc.Bancaria
CDICampanaTCDCxc.Bancaria=CDICampanaTCDCxc.TipoTarjeta
CDICampanaTCDCxc.TipoTarjeta=CDICampanaTCDCxc.Tc1
CDICampanaTCDCxc.Tc1=CDICampanaTCDCxc.Cs1
CDICampanaTCDCxc.Cs1=CDICampanaTCDCxc.Tc2
CDICampanaTCDCxc.Tc2=CDICampanaTCDCxc.Cs2
CDICampanaTCDCxc.Cs2=CDICampanaTCDCxc.FechaVencimiento
CDICampanaTCDCxc.FechaVencimiento=CDICampanaTCDCxc.cajero
CDICampanaTCDCxc.cajero=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDICampanaTCDCxc.ID,
CDICampanaTCDCxc.ID,=CDICampanaTCDCxc.Amex,
CDICampanaTCDCxc.Amex,=CDICampanaTCDCxc.Bancaria,
CDICampanaTCDCxc.Bancaria,=CDICampanaTCDCxc.TipoTarjeta,
CDICampanaTCDCxc.TipoTarjeta,=CDICampanaTCDCxc.Tc1,
CDICampanaTCDCxc.Tc1,=CDICampanaTCDCxc.Cs1,
CDICampanaTCDCxc.Cs1,=CDICampanaTCDCxc.Tc2,
CDICampanaTCDCxc.Tc2,=CDICampanaTCDCxc.Cs2,
CDICampanaTCDCxc.Cs2,=CDICampanaTCDCxc.FechaVencimiento,
CDICampanaTCDCxc.FechaVencimiento,=CDICampanaTCDCxc.cajero
CDICampanaTCDCxc.cajero=FROM CDICampanaTCDCxc
FROM CDICampanaTCDCxc=(Fin)
