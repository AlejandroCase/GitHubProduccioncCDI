[Vista.ListaCampos]
(Inicio)=CDIEquiposTorneos.Clave
CDIEquiposTorneos.Clave=CDIEquiposTorneos.Nombre
CDIEquiposTorneos.Nombre=CDIEquiposTorneos.CapitanSocio
CDIEquiposTorneos.CapitanSocio=CDIEquiposTorneos.SubCapitanSocio
CDIEquiposTorneos.SubCapitanSocio=CDIEquiposTorneos.PreferenciaDias
CDIEquiposTorneos.PreferenciaDias=CDIEquiposTorneos.PreferenciaHorario
CDIEquiposTorneos.PreferenciaHorario=CDIEquiposTorneos.Religioso
CDIEquiposTorneos.Religioso=CDIEquiposTorneos.Filial
CDIEquiposTorneos.Filial=CDIEquiposTorneos.EquipoFilial
CDIEquiposTorneos.EquipoFilial=CDIEquiposTorneos.EstatusEquipo
CDIEquiposTorneos.EstatusEquipo=CDIEquiposTorneos.Deporte
CDIEquiposTorneos.Deporte=CDIEquiposTorneos.RamaDepor
CDIEquiposTorneos.RamaDepor=CDIEquiposTorneos.Fuerza
CDIEquiposTorneos.Fuerza=CDIEquiposTorneos.Estatus
CDIEquiposTorneos.Estatus=CDIEquiposTorneos.Situacion
CDIEquiposTorneos.Situacion=CDIEquiposTorneos.SituacionFecha
CDIEquiposTorneos.SituacionFecha=CDIEquiposTorneos.SituacionUsuario
CDIEquiposTorneos.SituacionUsuario=CDIEquiposTorneos.Usuario
CDIEquiposTorneos.Usuario=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDIEquiposTorneos.Clave,
CDIEquiposTorneos.Clave,=CDIEquiposTorneos.Nombre,
CDIEquiposTorneos.Nombre,=CDIEquiposTorneos.CapitanSocio,
CDIEquiposTorneos.CapitanSocio,=CDIEquiposTorneos.SubCapitanSocio,
CDIEquiposTorneos.SubCapitanSocio,=CDIEquiposTorneos.PreferenciaDias,
CDIEquiposTorneos.PreferenciaDias,=CDIEquiposTorneos.PreferenciaHorario,
CDIEquiposTorneos.PreferenciaHorario,=CDIEquiposTorneos.Religioso,
CDIEquiposTorneos.Religioso,=CDIEquiposTorneos.Filial,
CDIEquiposTorneos.Filial,=CDIEquiposTorneos.EquipoFilial,
CDIEquiposTorneos.EquipoFilial,=CDIEquiposTorneos.EstatusEquipo,
CDIEquiposTorneos.EstatusEquipo,=CDIEquiposTorneos.Deporte,
CDIEquiposTorneos.Deporte,=CDIEquiposTorneos.RamaDepor,
CDIEquiposTorneos.RamaDepor,=CDIEquiposTorneos.Fuerza,
CDIEquiposTorneos.Fuerza,=CDIEquiposTorneos.Estatus,
CDIEquiposTorneos.Estatus,=CDIEquiposTorneos.Situacion,
CDIEquiposTorneos.Situacion,=CDIEquiposTorneos.SituacionFecha,
CDIEquiposTorneos.SituacionFecha,=CDIEquiposTorneos.SituacionUsuario,
CDIEquiposTorneos.SituacionUsuario,=CDIEquiposTorneos.Usuario
CDIEquiposTorneos.Usuario=FROM CDIEquiposTorneos
FROM CDIEquiposTorneos=(Fin)
