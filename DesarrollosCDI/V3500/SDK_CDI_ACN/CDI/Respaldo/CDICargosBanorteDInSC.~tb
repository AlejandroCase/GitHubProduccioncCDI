[Tabla.ListaCampos]
(Inicio)=Id
Id=Renglon
Renglon=Referencia
Referencia=NUMERO_CONTROL
NUMERO_CONTROL=NUMERO_TARJETA
NUMERO_TARJETA=FECHA_EXP
FECHA_EXP=MONTO
MONTO=ESTATUS
ESTATUS=CodAutorizacion
CodAutorizacion=MensajeRespuesta
MensajeRespuesta=CodRespuesta
CodRespuesta=Programada
Programada=Comentarios
Comentarios=EstatusProc
EstatusProc=IdCxcCob
IdCxcCob=IDRecCxc
IDRecCxc=IDRecVta
IDRecVta=(Fin)

[Tabla.SQL]
(Inicio)=SELECT
SELECT=Id,
Id,=Renglon,
Renglon,=Referencia,
Referencia,=NUMERO_CONTROL,
NUMERO_CONTROL,=NUMERO_TARJETA,
NUMERO_TARJETA,=FECHA_EXP,
FECHA_EXP,=MONTO,
MONTO,=ESTATUS,
ESTATUS,=CodAutorizacion,
CodAutorizacion,=MensajeRespuesta,
MensajeRespuesta,=CodRespuesta,
CodRespuesta,=Programada,
Programada,=Comentarios,
Comentarios,=EstatusProc,
EstatusProc,=IdCxcCob,
IdCxcCob,=IDRecCxc,
IDRecCxc,=IDRecVta
IDRecVta=FROM CDICargosBanorteDInSC
FROM CDICargosBanorteDInSC=(Fin)

