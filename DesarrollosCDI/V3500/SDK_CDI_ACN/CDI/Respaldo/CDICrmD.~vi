[Vista.ListaCampos]
(Inicio)=CDICrmD.ID
CDICrmD.ID=CDICrmD.Renglon
CDICrmD.Renglon=CDICrmD.NIPCDI
CDICrmD.NIPCDI=CDICrmD.Cliente
CDICrmD.Cliente=CDICrmD.Membresia
CDICrmD.Membresia=CDICrmD.Socio
CDICrmD.Socio=CDICrmD.Procesar
CDICrmD.Procesar=CDICrmD.AgenteAnt
CDICrmD.AgenteAnt=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDICrmD.ID,
CDICrmD.ID,=CDICrmD.Renglon,
CDICrmD.Renglon,=CDICrmD.NIPCDI,
CDICrmD.NIPCDI,=CDICrmD.Cliente,
CDICrmD.Cliente,=CDICrmD.Membresia,
CDICrmD.Membresia,=CDICrmD.Socio,
CDICrmD.Socio,=CDICrmD.Procesar,
CDICrmD.Procesar,=CDICrmD.AgenteAnt
CDICrmD.AgenteAnt=FROM CDICrmD
FROM CDICrmD=(Fin)

[Vista.CamposBusquedaRapida]
(Inicio)=CDICrmD.Cliente
