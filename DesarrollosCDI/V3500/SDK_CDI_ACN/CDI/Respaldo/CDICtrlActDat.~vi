[Vista.ListaCampos]
(Inicio)=CDICtrlActDat.Cliente
CDICtrlActDat.Cliente=CDICtrlActDat.Socio
CDICtrlActDat.Socio=CDICtrlActDat.Membresia
CDICtrlActDat.Membresia=CDICtrlActDat.ActDatos
CDICtrlActDat.ActDatos=CDICtrlActDat.ActValida
CDICtrlActDat.ActValida=CDICtrlActDat.ActPago
CDICtrlActDat.ActPago=CDICtrlActDat.ActImpCred
CDICtrlActDat.ActImpCred=CDICtrlActDat.Rehd
CDICtrlActDat.Rehd=CDICtrlActDat.RegChip
CDICtrlActDat.RegChip=CDICtrlActDat.Foto
CDICtrlActDat.Foto=CDICtrlActDat.EntregaCred
CDICtrlActDat.EntregaCred=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDICtrlActDat.Cliente,
CDICtrlActDat.Cliente,=CDICtrlActDat.Socio,
CDICtrlActDat.Socio,=CDICtrlActDat.Membresia,
CDICtrlActDat.Membresia,=CDICtrlActDat.ActDatos,
CDICtrlActDat.ActDatos,=CDICtrlActDat.ActValida,
CDICtrlActDat.ActValida,=CDICtrlActDat.ActPago,
CDICtrlActDat.ActPago,=CDICtrlActDat.ActImpCred,
CDICtrlActDat.ActImpCred,=CDICtrlActDat.Rehd,
CDICtrlActDat.Rehd,=CDICtrlActDat.RegChip,
CDICtrlActDat.RegChip,=CDICtrlActDat.Foto,
CDICtrlActDat.Foto,=CDICtrlActDat.EntregaCred
CDICtrlActDat.EntregaCred=FROM CDICtrlActDat
FROM CDICtrlActDat=(Fin)

