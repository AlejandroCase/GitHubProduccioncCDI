[Vista.SQL]
(Inicio)=SELECT Tarea.Cliente,
SELECT Tarea.Cliente,=Tarea.Usuario,
Tarea.Usuario,=Tarea.Asunto,
Tarea.Asunto,=Tarea.Comentarios,
Tarea.Comentarios,=Tarea.Estado,
Tarea.Estado,=Tarea.Vencimiento,
Tarea.Vencimiento,=Tarea.Prioridad,
Tarea.Prioridad,=Tarea.FechaInicio,
Tarea.FechaInicio,=Tarea.Forzosa,
Tarea.Forzosa,=Tarea.Revision,
Tarea.Revision,=Tarea.Tipo,
Tarea.Tipo,=Tarea.UsuarioAsignado,
Tarea.UsuarioAsignado,=Tarea.Contacto FROM
Tarea.Contacto FROM=Tarea
Tarea=LEFT OUTER JOIN Usuario ON Tarea.UsuarioResponsable<IGUAL>Usuario.Usuario
LEFT OUTER JOIN Usuario ON Tarea.UsuarioResponsable<IGUAL>Usuario.Usuario=LEFT OUTER JOIN Usuario UsuarioDestino ON Tarea.UsuarioAsignado<IGUAL>UsuarioDestino.Usuario
LEFT OUTER JOIN Usuario UsuarioDestino ON Tarea.UsuarioAsignado<IGUAL>UsuarioDestino.Usuario=(Fin)













[Vista.ListaCampos]
(Inicio)=Cliente
Cliente=Usuario
Usuario=Asunto
Asunto=Comentarios
Comentarios=Estado
Estado=Vencimiento
Vencimiento=Prioridad
Prioridad=FechaInicio
FechaInicio=Forzosa
Forzosa=Revision
Revision=Tipo
Tipo=UsuarioAsignado
UsuarioAsignado=Contacto
Contacto=(Fin)
