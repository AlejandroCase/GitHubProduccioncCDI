[Vista.ListaCampos]
(Inicio)=CDIBoniCompras.Renglon
CDIBoniCompras.Renglon=CDIBoniCompras.Mov
CDIBoniCompras.Mov=CDIBoniCompras.Movid
CDIBoniCompras.Movid=CDIBoniCompras.FechaEmision
CDIBoniCompras.FechaEmision=CDIBoniCompras.Proveedor
CDIBoniCompras.Proveedor=CDIBoniCompras.Origen
CDIBoniCompras.Origen=CDIBoniCompras.OrigenID
CDIBoniCompras.OrigenID=CDIBoniCompras.Articulo
CDIBoniCompras.Articulo=CDIBoniCompras.Cantidad
CDIBoniCompras.Cantidad=CDIBoniCompras.Costo
CDIBoniCompras.Costo=CDIBoniCompras.Impuesto1
CDIBoniCompras.Impuesto1=CDIBoniCompras.ClavePresupuestal
CDIBoniCompras.ClavePresupuestal=CDIBoniCompras.MovOC
CDIBoniCompras.MovOC=CDIBoniCompras.MovIDOc
CDIBoniCompras.MovIDOc=CDIBoniCompras.ArticuloOC
CDIBoniCompras.ArticuloOC=CDIBoniCompras.CantidadOC
CDIBoniCompras.CantidadOC=CDIBoniCompras.CostoOC
CDIBoniCompras.CostoOC=CDIBoniCompras.Impuesto1OC
CDIBoniCompras.Impuesto1OC=CDIBoniCompras.ClavePresupuestalOC
CDIBoniCompras.ClavePresupuestalOC=CDIBoniCompras.CDICantBonif
CDIBoniCompras.CDICantBonif=CDIBoniCompras.CDICostBonif
CDIBoniCompras.CDICostBonif=CDIBoniCompras.IDOC
CDIBoniCompras.IDOC=CDIBoniCompras.IDBonif
CDIBoniCompras.IDBonif=CDIBoniCompras.ID
CDIBoniCompras.ID=CDIBoniCompras.Almacen
CDIBoniCompras.Almacen=CDIBoniCompras.Impuesto2
CDIBoniCompras.Impuesto2=CDIBoniCompras.Impuesto3
CDIBoniCompras.Impuesto3=CDIBoniCompras.DescuentoTipo
CDIBoniCompras.DescuentoTipo=CDIBoniCompras.Paquete
CDIBoniCompras.Paquete=CDIBoniCompras.PaqueteCantidad
CDIBoniCompras.PaqueteCantidad=CDIBoniCompras.Observaciones
CDIBoniCompras.Observaciones=Art.Articulo
Art.Articulo=Art.Descripcion1
Art.Descripcion1=Art.Unidad
Art.Unidad=Prov.Proveedor
Prov.Proveedor=Prov.Nombre
Prov.Nombre=(Fin)

[Vista.ListaTablas]
(Inicio)=CDIBoniCompras
CDIBoniCompras=Art
Art=Prov
Prov=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDIBoniCompras.Renglon,
CDIBoniCompras.Renglon,=CDIBoniCompras.Mov,
CDIBoniCompras.Mov,=CDIBoniCompras.Movid,
CDIBoniCompras.Movid,=CDIBoniCompras.FechaEmision,
CDIBoniCompras.FechaEmision,=CDIBoniCompras.Proveedor,
CDIBoniCompras.Proveedor,=CDIBoniCompras.Origen,
CDIBoniCompras.Origen,=CDIBoniCompras.OrigenID,
CDIBoniCompras.OrigenID,=CDIBoniCompras.Articulo,
CDIBoniCompras.Articulo,=CDIBoniCompras.Cantidad,
CDIBoniCompras.Cantidad,=CDIBoniCompras.Costo,
CDIBoniCompras.Costo,=CDIBoniCompras.Impuesto1,
CDIBoniCompras.Impuesto1,=CDIBoniCompras.ClavePresupuestal,
CDIBoniCompras.ClavePresupuestal,=CDIBoniCompras.MovOC,
CDIBoniCompras.MovOC,=CDIBoniCompras.MovIDOc,
CDIBoniCompras.MovIDOc,=CDIBoniCompras.ArticuloOC,
CDIBoniCompras.ArticuloOC,=CDIBoniCompras.CantidadOC,
CDIBoniCompras.CantidadOC,=CDIBoniCompras.CostoOC,
CDIBoniCompras.CostoOC,=CDIBoniCompras.Impuesto1OC,
CDIBoniCompras.Impuesto1OC,=CDIBoniCompras.ClavePresupuestalOC,
CDIBoniCompras.ClavePresupuestalOC,=CDIBoniCompras.CDICantBonif,
CDIBoniCompras.CDICantBonif,=CDIBoniCompras.CDICostBonif,
CDIBoniCompras.CDICostBonif,=CDIBoniCompras.IDOC,
CDIBoniCompras.IDOC,=CDIBoniCompras.IDBonif,
CDIBoniCompras.IDBonif,=CDIBoniCompras.ID,
CDIBoniCompras.ID,=CDIBoniCompras.Almacen,
CDIBoniCompras.Almacen,=CDIBoniCompras.Impuesto2,
CDIBoniCompras.Impuesto2,=CDIBoniCompras.Impuesto3,
CDIBoniCompras.Impuesto3,=CDIBoniCompras.DescuentoTipo,
CDIBoniCompras.DescuentoTipo,=CDIBoniCompras.Paquete,
CDIBoniCompras.Paquete,=CDIBoniCompras.PaqueteCantidad,
CDIBoniCompras.PaqueteCantidad,=CDIBoniCompras.Observaciones,
CDIBoniCompras.Observaciones,=Art.Articulo,
Art.Articulo,=Art.Descripcion1,
Art.Descripcion1,=Art.Unidad,
Art.Unidad,=Prov.Proveedor,
Prov.Proveedor,=Prov.Nombre
Prov.Nombre=FROM CDIBoniCompras
FROM CDIBoniCompras=JOIN Prov ON CDIBoniCompras.Proveedor<IGUAL>Prov.Proveedor
JOIN Prov ON CDIBoniCompras.Proveedor<IGUAL>Prov.Proveedor=JOIN Art ON CDIBoniCompras.Articulo<IGUAL>Art.Articulo
JOIN Art ON CDIBoniCompras.Articulo<IGUAL>Art.Articulo=(Fin)

[Vista.ListaRelaciones]
(Inicio)=CDIBoniCompras.Prov
CDIBoniCompras.Prov=CDIBoniCompras.Art
CDIBoniCompras.Art=Art.Prov
Art.Prov=(Fin)

[Vista.CamposBusquedaRapida]
(Inicio)=Art.Articulo
Art.Articulo=Art.Descripcion1
Art.Descripcion1=CDIBoniCompras.Movid
CDIBoniCompras.Movid=CDIBoniCompras.OrigenID
CDIBoniCompras.OrigenID=CDIBoniCompras.MovIDOc
CDIBoniCompras.MovIDOc=(Fin)
