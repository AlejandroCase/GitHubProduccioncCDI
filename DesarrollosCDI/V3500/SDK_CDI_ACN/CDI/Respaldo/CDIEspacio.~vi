[Vista.ListaCampos]
(Inicio)=CDIEspacio.ID
CDIEspacio.ID=CDIEspacio.Empresa
CDIEspacio.Empresa=CDIEspacio.Mov
CDIEspacio.Mov=CDIEspacio.MovID
CDIEspacio.MovID=CDIEspacio.FechaEmision
CDIEspacio.FechaEmision=CDIEspacio.UltimoCambio
CDIEspacio.UltimoCambio=CDIEspacio.Concepto
CDIEspacio.Concepto=CDIEspacio.Proyecto
CDIEspacio.Proyecto=CDIEspacio.UEN
CDIEspacio.UEN=CDIEspacio.Usuario
CDIEspacio.Usuario=CDIEspacio.Autorizacion
CDIEspacio.Autorizacion=CDIEspacio.Referencia
CDIEspacio.Referencia=CDIEspacio.Observaciones
CDIEspacio.Observaciones=CDIEspacio.Causa
CDIEspacio.Causa=CDIEspacio.Estatus
CDIEspacio.Estatus=CDIEspacio.Situacion
CDIEspacio.Situacion=CDIEspacio.SituacionFecha
CDIEspacio.SituacionFecha=CDIEspacio.SituacionUsuario
CDIEspacio.SituacionUsuario=CDIEspacio.SituacionNota
CDIEspacio.SituacionNota=CDIEspacio.OrigenTipo
CDIEspacio.OrigenTipo=CDIEspacio.Origen
CDIEspacio.Origen=CDIEspacio.OrigenID
CDIEspacio.OrigenID=CDIEspacio.Ejercicio
CDIEspacio.Ejercicio=CDIEspacio.Periodo
CDIEspacio.Periodo=CDIEspacio.FechaRegistro
CDIEspacio.FechaRegistro=CDIEspacio.FechaConclusion
CDIEspacio.FechaConclusion=CDIEspacio.FechaCancelacion
CDIEspacio.FechaCancelacion=CDIEspacio.Sucursal
CDIEspacio.Sucursal=CDIEspacio.Moneda
CDIEspacio.Moneda=CDIEspacio.TipoCambio
CDIEspacio.TipoCambio=CDIEspacio.FechaAplicacion
CDIEspacio.FechaAplicacion=CDIEspacio.NIPCDI
CDIEspacio.NIPCDI=CDIEspacio.Cliente
CDIEspacio.Cliente=CDIEspacio.Membresia
CDIEspacio.Membresia=CDIEspacio.Socio
CDIEspacio.Socio=CDIEspacio.FechaIngreso
CDIEspacio.FechaIngreso=CDIEspacio.FechaVigencia
CDIEspacio.FechaVigencia=CDIEspacio.Categoria
CDIEspacio.Categoria=CDIEspacio.ListaPreciosCuotaA
CDIEspacio.ListaPreciosCuotaA=CDIEspacio.ListaPreciosCuotaM
CDIEspacio.ListaPreciosCuotaM=CDIEspacio.Justificacion
CDIEspacio.Justificacion=CDIEspacio.BajaTotal
CDIEspacio.BajaTotal=CDIEspacio.CDIEstado
CDIEspacio.CDIEstado=CDIEspacio.Comentarios
CDIEspacio.Comentarios=CDIEspacio.Estado
CDIEspacio.Estado=CDIEspacio.CDIFlujoAutorizacion
CDIEspacio.CDIFlujoAutorizacion=CDIEspacio.CDIPermiteEditar
CDIEspacio.CDIPermiteEditar=CDIEspacio.ComentBloqueo
CDIEspacio.ComentBloqueo=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDIEspacio.ID,
CDIEspacio.ID,=CDIEspacio.Empresa,
CDIEspacio.Empresa,=CDIEspacio.Mov,
CDIEspacio.Mov,=CDIEspacio.MovID,
CDIEspacio.MovID,=CDIEspacio.FechaEmision,
CDIEspacio.FechaEmision,=CDIEspacio.UltimoCambio,
CDIEspacio.UltimoCambio,=CDIEspacio.Concepto,
CDIEspacio.Concepto,=CDIEspacio.Proyecto,
CDIEspacio.Proyecto,=CDIEspacio.UEN,
CDIEspacio.UEN,=CDIEspacio.Usuario,
CDIEspacio.Usuario,=CDIEspacio.Autorizacion,
CDIEspacio.Autorizacion,=CDIEspacio.Referencia,
CDIEspacio.Referencia,=CDIEspacio.Observaciones,
CDIEspacio.Observaciones,=CDIEspacio.Causa,
CDIEspacio.Causa,=CDIEspacio.Estatus,
CDIEspacio.Estatus,=CDIEspacio.Situacion,
CDIEspacio.Situacion,=CDIEspacio.SituacionFecha,
CDIEspacio.SituacionFecha,=CDIEspacio.SituacionUsuario,
CDIEspacio.SituacionUsuario,=CDIEspacio.SituacionNota,
CDIEspacio.SituacionNota,=CDIEspacio.OrigenTipo,
CDIEspacio.OrigenTipo,=CDIEspacio.Origen,
CDIEspacio.Origen,=CDIEspacio.OrigenID,
CDIEspacio.OrigenID,=CDIEspacio.Ejercicio,
CDIEspacio.Ejercicio,=CDIEspacio.Periodo,
CDIEspacio.Periodo,=CDIEspacio.FechaRegistro,
CDIEspacio.FechaRegistro,=CDIEspacio.FechaConclusion,
CDIEspacio.FechaConclusion,=CDIEspacio.FechaCancelacion,
CDIEspacio.FechaCancelacion,=CDIEspacio.Sucursal,
CDIEspacio.Sucursal,=CDIEspacio.Moneda,
CDIEspacio.Moneda,=CDIEspacio.TipoCambio,
CDIEspacio.TipoCambio,=CDIEspacio.FechaAplicacion,
CDIEspacio.FechaAplicacion,=CDIEspacio.NIPCDI,
CDIEspacio.NIPCDI,=CDIEspacio.Cliente,
