[Vista.ListaCampos]
(Inicio)=CDIMovsSocio.id
CDIMovsSocio.id=CDIMovsSocio.Mov
CDIMovsSocio.Mov=CDIMovsSocio.FechaEmision
CDIMovsSocio.FechaEmision=CDIMovsSocio.Estatus
CDIMovsSocio.Estatus=CDIMovsSocio.Cliente
CDIMovsSocio.Cliente=CDIMovsSocio.Socio
CDIMovsSocio.Socio=CDIMovsSocio.Concepto
CDIMovsSocio.Concepto=CDIMovsSocio.Referencia
CDIMovsSocio.Referencia=CDIMovsSocio.Observaciones
CDIMovsSocio.Observaciones=CDIMovsSocio.Usuario
CDIMovsSocio.Usuario=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDIMovsSocio.id,
CDIMovsSocio.id,=CDIMovsSocio.Mov,
CDIMovsSocio.Mov,=CDIMovsSocio.FechaEmision,
CDIMovsSocio.FechaEmision,=CDIMovsSocio.Estatus,
CDIMovsSocio.Estatus,=CDIMovsSocio.Cliente,
CDIMovsSocio.Cliente,=CDIMovsSocio.Socio,
CDIMovsSocio.Socio,=CDIMovsSocio.Concepto,
CDIMovsSocio.Concepto,=CDIMovsSocio.Referencia,
CDIMovsSocio.Referencia,=CDIMovsSocio.Observaciones,
CDIMovsSocio.Observaciones,=CDIMovsSocio.Usuario
CDIMovsSocio.Usuario=FROM CDIMovsSocio
FROM CDIMovsSocio=(Fin)

