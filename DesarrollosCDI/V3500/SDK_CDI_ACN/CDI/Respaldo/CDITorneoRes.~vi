[Vista.ListaCampos]
(Inicio)=CDITorneoRes.Id
CDITorneoRes.Id=CDITorneoRes.ClaveTorneo
CDITorneoRes.ClaveTorneo=CDITorneoRes.NoVuelta
CDITorneoRes.NoVuelta=CDITorneoRes.NoJornada
CDITorneoRes.NoJornada=CDITorneoRes.Fecha
CDITorneoRes.Fecha=CDITorneoRes.HoraInicio
CDITorneoRes.HoraInicio=CDITorneoRes.HoraTermino
CDITorneoRes.HoraTermino=CDITorneoRes.EquipoLocal
CDITorneoRes.EquipoLocal=CDITorneoRes.ForfeitL
CDITorneoRes.ForfeitL=CDITorneoRes.EquipoVisitante
CDITorneoRes.EquipoVisitante=CDITorneoRes.ForfeitV
CDITorneoRes.ForfeitV=CDITorneoRes.Arbitro1
CDITorneoRes.Arbitro1=CDITorneoRes.Arbitro2
CDITorneoRes.Arbitro2=CDITorneoRes.Arbitro3
CDITorneoRes.Arbitro3=CDITorneoRes.Arbitro4
CDITorneoRes.Arbitro4=CDITorneoRes.Anotador
CDITorneoRes.Anotador=CDITorneoRes.MarcadorLocal
CDITorneoRes.MarcadorLocal=CDITorneoRes.MarcadorVisitante
CDITorneoRes.MarcadorVisitante=CDITorneoRes.TiemposFueraLocal
CDITorneoRes.TiemposFueraLocal=CDITorneoRes.TiemposFueraVisitante
CDITorneoRes.TiemposFueraVisitante=CDITorneoRes.Usuario
CDITorneoRes.Usuario=CDITorneoRes.Estatus
CDITorneoRes.Estatus=CDITorneoRes.Situacion
CDITorneoRes.Situacion=CDITorneoRes.SituacionFecha
CDITorneoRes.SituacionFecha=CDITorneoRes.SituacionUsuario
CDITorneoRes.SituacionUsuario=CDITorneoRes.Empresa
CDITorneoRes.Empresa=CDITorneoRes.Proyecto
CDITorneoRes.Proyecto=CDITorneoRes.RenglonId
CDITorneoRes.RenglonId=CDITorneoRes.comentarios
CDITorneoRes.comentarios=CDITorneoRes.IdJornadaEnc
CDITorneoRes.IdJornadaEnc=CDITorneoRes.RenglonIDJornadaEnc
CDITorneoRes.RenglonIDJornadaEnc=CDITorneoRes.IdResultado
CDITorneoRes.IdResultado=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDITorneoRes.Id,
CDITorneoRes.Id,=CDITorneoRes.ClaveTorneo,
CDITorneoRes.ClaveTorneo,=CDITorneoRes.NoVuelta,
CDITorneoRes.NoVuelta,=CDITorneoRes.NoJornada,
CDITorneoRes.NoJornada,=CDITorneoRes.Fecha,
CDITorneoRes.Fecha,=CDITorneoRes.HoraInicio,
CDITorneoRes.HoraInicio,=CDITorneoRes.HoraTermino,
CDITorneoRes.HoraTermino,=CDITorneoRes.EquipoLocal,
CDITorneoRes.EquipoLocal,=CDITorneoRes.ForfeitL,
CDITorneoRes.ForfeitL,=CDITorneoRes.EquipoVisitante,
CDITorneoRes.EquipoVisitante,=CDITorneoRes.ForfeitV,
CDITorneoRes.ForfeitV,=CDITorneoRes.Arbitro1,
CDITorneoRes.Arbitro1,=CDITorneoRes.Arbitro2,
CDITorneoRes.Arbitro2,=CDITorneoRes.Arbitro3,
CDITorneoRes.Arbitro3,=CDITorneoRes.Arbitro4,
CDITorneoRes.Arbitro4,=CDITorneoRes.Anotador,
CDITorneoRes.Anotador,=CDITorneoRes.MarcadorLocal,
CDITorneoRes.MarcadorLocal,=CDITorneoRes.MarcadorVisitante,
CDITorneoRes.MarcadorVisitante,=CDITorneoRes.TiemposFueraLocal,
CDITorneoRes.TiemposFueraLocal,=CDITorneoRes.TiemposFueraVisitante,
CDITorneoRes.TiemposFueraVisitante,=CDITorneoRes.Usuario,
CDITorneoRes.Usuario,=CDITorneoRes.Estatus,
CDITorneoRes.Estatus,=CDITorneoRes.Situacion,
CDITorneoRes.Situacion,=CDITorneoRes.SituacionFecha,
CDITorneoRes.SituacionFecha,=CDITorneoRes.SituacionUsuario,
CDITorneoRes.SituacionUsuario,=CDITorneoRes.Empresa,
CDITorneoRes.Empresa,=CDITorneoRes.Proyecto,
CDITorneoRes.Proyecto,=CDITorneoRes.RenglonId,
CDITorneoRes.RenglonId,=CDITorneoRes.comentarios,
CDITorneoRes.comentarios,=CDITorneoRes.IdJornadaEnc,
CDITorneoRes.IdJornadaEnc,=CDITorneoRes.RenglonIDJornadaEnc,
CDITorneoRes.RenglonIDJornadaEnc,=CDITorneoRes.IdResultado
CDITorneoRes.IdResultado=FROM CDITorneoRes
FROM CDITorneoRes=(Fin)
