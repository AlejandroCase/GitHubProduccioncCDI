[Vista.ListaCampos]
(Inicio)=CDICOC.ID
CDICOC.ID=CDICOC.Proyecto
CDICOC.Proyecto=CDICOC.mov
CDICOC.mov=CDICOC.movid
CDICOC.movid=CDICOC.origen
CDICOC.origen=CDICOC.origenid
CDICOC.origenid=CDICOC.FechaEmision
CDICOC.FechaEmision=CDICOC.usuario
CDICOC.usuario=CDICOC.moneda
CDICOC.moneda=CDICOC.TipoCambio
CDICOC.TipoCambio=CDICOC.Almacen
CDICOC.Almacen=CDICOC.Importe
CDICOC.Importe=CDICOC.impuestos
CDICOC.impuestos=CDICOC.FechaRegistro
CDICOC.FechaRegistro=CDICOC.FechaCancelacion
CDICOC.FechaCancelacion=CDICOC.FechaProveedor
CDICOC.FechaProveedor=CDICOC.FechaRequerida
CDICOC.FechaRequerida=CDICOC.CDIEstado
CDICOC.CDIEstado=CDICOC.Estatus
CDICOC.Estatus=CDICOC.Articulo
CDICOC.Articulo=CDICOC.Cantidad
CDICOC.Cantidad=CDICOC.CantidadA
CDICOC.CantidadA=CDICOC.CantidadPendiente
CDICOC.CantidadPendiente=CDICOC.Costo
CDICOC.Costo=CDICOC.Unidad
CDICOC.Unidad=CDICOC.FechaEntrega
CDICOC.FechaEntrega=CDICOC.Ejercicio
CDICOC.Ejercicio=CDICOC.periodo
CDICOC.periodo=CDICOC.Renglon
CDICOC.Renglon=CDICOC.Descripcion1
CDICOC.Descripcion1=CDICOC.ClavePresupuestal
CDICOC.ClavePresupuestal=CDICOC.Proveedor
CDICOC.Proveedor=CDICOC.Nombre
CDICOC.Nombre=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDICOC.ID,
CDICOC.ID,=CDICOC.Proyecto,
CDICOC.Proyecto,=CDICOC.mov,
CDICOC.mov,=CDICOC.movid,
CDICOC.movid,=CDICOC.origen,
CDICOC.origen,=CDICOC.origenid,
CDICOC.origenid,=CDICOC.FechaEmision,
CDICOC.FechaEmision,=CDICOC.usuario,
CDICOC.usuario,=CDICOC.moneda,
CDICOC.moneda,=CDICOC.TipoCambio,
CDICOC.TipoCambio,=CDICOC.Almacen,
CDICOC.Almacen,=CDICOC.Importe,
CDICOC.Importe,=CDICOC.impuestos,
CDICOC.impuestos,=CDICOC.FechaRegistro,
CDICOC.FechaRegistro,=CDICOC.FechaCancelacion,
CDICOC.FechaCancelacion,=CDICOC.FechaProveedor,
CDICOC.FechaProveedor,=CDICOC.FechaRequerida,
CDICOC.FechaRequerida,=CDICOC.CDIEstado,
CDICOC.CDIEstado,=CDICOC.Estatus,
CDICOC.Estatus,=CDICOC.Articulo,
CDICOC.Articulo,=CDICOC.Cantidad,
CDICOC.Cantidad,=CDICOC.CantidadA,
CDICOC.CantidadA,=CDICOC.CantidadPendiente,
CDICOC.CantidadPendiente,=CDICOC.Costo,
CDICOC.Costo,=CDICOC.Unidad,
CDICOC.Unidad,=CDICOC.FechaEntrega,
CDICOC.FechaEntrega,=CDICOC.Ejercicio,
CDICOC.Ejercicio,=CDICOC.periodo,
CDICOC.periodo,=CDICOC.Renglon,
CDICOC.Renglon,=CDICOC.Descripcion1,
CDICOC.Descripcion1,=CDICOC.ClavePresupuestal,
CDICOC.ClavePresupuestal,=CDICOC.Proveedor,
CDICOC.Proveedor,=CDICOC.Nombre
CDICOC.Nombre=FROM CDICOC
FROM CDICOC=(Fin)
