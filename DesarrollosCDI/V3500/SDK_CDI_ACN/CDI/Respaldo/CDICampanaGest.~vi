[Vista.ListaCampos]
(Inicio)=CDICampanaGest.Id
CDICampanaGest.Id=CDICampanaGest.RneglonId
CDICampanaGest.RneglonId=CDICampanaGest.Accion
CDICampanaGest.Accion=CDICampanaGest.Acuerdo
CDICampanaGest.Acuerdo=CDICampanaGest.Contacto
CDICampanaGest.Contacto=CDICampanaGest.FechaCompromiso
CDICampanaGest.FechaCompromiso=CDICampanaGest.FormaPago
CDICampanaGest.FormaPago=CDICampanaGest.Tel
CDICampanaGest.Tel=CDICampanaGest.Telefono
CDICampanaGest.Telefono=CDICampanaGest.Cel
CDICampanaGest.Cel=CDICampanaGest.Celular
CDICampanaGest.Celular=CDICampanaGest.TelOf
CDICampanaGest.TelOf=CDICampanaGest.TelOfna
CDICampanaGest.TelOfna=CDICampanaGest.Dir
CDICampanaGest.Dir=CDICampanaGest.Direccion
CDICampanaGest.Direccion=CDICampanaGest.Cor
CDICampanaGest.Cor=CDICampanaGest.Correo
CDICampanaGest.Correo=CDICampanaGest.DireccionNumero
CDICampanaGest.DireccionNumero=CDICampanaGest.DireccionNumeroInt
CDICampanaGest.DireccionNumeroInt=CDICampanaGest.EntreCalles
CDICampanaGest.EntreCalles=CDICampanaGest.Delegacion
CDICampanaGest.Delegacion=CDICampanaGest.Colonia
CDICampanaGest.Colonia=CDICampanaGest.Poblacion
CDICampanaGest.Poblacion=CDICampanaGest.Estado
CDICampanaGest.Estado=CDICampanaGest.Pais
CDICampanaGest.Pais=CDICampanaGest.CodigoPostal
CDICampanaGest.CodigoPostal=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDICampanaGest.Id,
CDICampanaGest.Id,=CDICampanaGest.RneglonId,
CDICampanaGest.RneglonId,=CDICampanaGest.Accion,
CDICampanaGest.Accion,=CDICampanaGest.Acuerdo,
CDICampanaGest.Acuerdo,=CDICampanaGest.Contacto,
CDICampanaGest.Contacto,=CDICampanaGest.FechaCompromiso,
CDICampanaGest.FechaCompromiso,=CDICampanaGest.FormaPago,
CDICampanaGest.FormaPago,=CDICampanaGest.Tel,
CDICampanaGest.Tel,=CDICampanaGest.Telefono,
CDICampanaGest.Telefono,=CDICampanaGest.Cel,
CDICampanaGest.Cel,=CDICampanaGest.Celular,
CDICampanaGest.Celular,=CDICampanaGest.TelOf,
CDICampanaGest.TelOf,=CDICampanaGest.TelOfna,
CDICampanaGest.TelOfna,=CDICampanaGest.Dir,
CDICampanaGest.Dir,=CDICampanaGest.Direccion,
CDICampanaGest.Direccion,=CDICampanaGest.Cor,
CDICampanaGest.Cor,=CDICampanaGest.Correo,
CDICampanaGest.Correo,=CDICampanaGest.DireccionNumero,
CDICampanaGest.DireccionNumero,=CDICampanaGest.DireccionNumeroInt,
CDICampanaGest.DireccionNumeroInt,=CDICampanaGest.EntreCalles,
CDICampanaGest.EntreCalles,=CDICampanaGest.Delegacion,
CDICampanaGest.Delegacion,=CDICampanaGest.Colonia,
CDICampanaGest.Colonia,=CDICampanaGest.Poblacion,
CDICampanaGest.Poblacion,=CDICampanaGest.Estado,
CDICampanaGest.Estado,=CDICampanaGest.Pais,
CDICampanaGest.Pais,=CDICampanaGest.CodigoPostal
CDICampanaGest.CodigoPostal=FROM CDICampanaGest
FROM CDICampanaGest=(Fin)
