[Tabla.ListaCampos]
(Inicio)=ID
ID=Proyecto
Proyecto=mov
mov=movid
movid=origen
origen=origenid
origenid=FechaEmision
FechaEmision=usuario
usuario=moneda
moneda=TipoCambio
TipoCambio=Almacen
Almacen=Importe
Importe=impuestos
impuestos=FechaRegistro
FechaRegistro=FechaCancelacion
FechaCancelacion=FechaProveedor
FechaProveedor=FechaRequerida
FechaRequerida=CDIEstado
CDIEstado=Estatus
Estatus=Ejercicio
Ejercicio=periodo
periodo=Proveedor
Proveedor=Nombre
Nombre=Articulo
Articulo=Cantidad
Cantidad=CantidadA
CantidadA=CantidadPendiente
CantidadPendiente=Costo
Costo=Unidad
Unidad=FechaEntrega
FechaEntrega=Renglon
Renglon=Descripcion1
Descripcion1=ClavePresupuestal
ClavePresupuestal=situacion
situacion=situacionfecha
situacionfecha=situacionusuario
situacionusuario=(Fin)

[Tabla.SQL]
(Inicio)=SELECT
SELECT=ID,
ID,=Proyecto,
Proyecto,=mov,
mov,=movid,
movid,=origen,
origen,=origenid,
origenid,=FechaEmision,
FechaEmision,=usuario,
usuario,=moneda,
moneda,=TipoCambio,
TipoCambio,=Almacen,
Almacen,=Importe,
Importe,=impuestos,
impuestos,=FechaRegistro,
FechaRegistro,=FechaCancelacion,
FechaCancelacion,=FechaProveedor,
FechaProveedor,=FechaRequerida,
FechaRequerida,=CDIEstado,
CDIEstado,=Estatus,
Estatus,=Ejercicio,
Ejercicio,=periodo,
periodo,=Proveedor,
Proveedor,=Nombre,
Nombre,=Articulo,
Articulo,=Cantidad,
Cantidad,=CantidadA,
CantidadA,=CantidadPendiente,
CantidadPendiente,=Costo,
Costo,=Unidad,
Unidad,=FechaEntrega,
FechaEntrega,=Renglon,
Renglon,=Descripcion1,
Descripcion1,=ClavePresupuestal,
ClavePresupuestal,=situacion,
situacion,=situacionfecha,
situacionfecha,=situacionusuario
situacionusuario=FROM CDICReqEnc
FROM CDICReqEnc=(Fin)

