[Tabla.ListaCampos]
(Inicio)=cliente
cliente=socio
socio=nombre
nombre=membresia
membresia=tipo
tipo=categoria
categoria=subcategoria
subcategoria=aporta
aporta=clase
clase=actividad
actividad=sede
sede=EstatusCte
EstatusCte=mov
mov=movid
movid=fechaemision
fechaemision=importe
importe=impuestos
impuestos=saldo
saldo=ImporteDoc
ImporteDoc=EstatusVta
EstatusVta=ejercicio
ejercicio=periodo
periodo=Valor
Valor=proyecto
proyecto=(Fin)

[Tabla.SQL]
(Inicio)=SELECT
SELECT=cliente,
cliente,=socio,
socio,=nombre,
nombre,=membresia,
membresia,=tipo,
tipo,=categoria,
categoria,=subcategoria,
subcategoria,=aporta,
aporta,=clase,
clase,=actividad,
actividad,=sede,
sede,=EstatusCte,
EstatusCte,=mov,
mov,=movid,
movid,=fechaemision,
fechaemision,=importe,
importe,=impuestos,
impuestos,=saldo,
saldo,=ImporteDoc,
ImporteDoc,=EstatusVta,
EstatusVta,=ejercicio,
ejercicio,=periodo,
periodo,=Valor,
Valor,=proyecto
proyecto=FROM cdicteclascxc
FROM cdicteclascxc=(Fin)
