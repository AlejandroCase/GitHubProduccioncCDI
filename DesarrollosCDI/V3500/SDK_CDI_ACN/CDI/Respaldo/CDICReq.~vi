[Vista.ListaCampos]
(Inicio)=CDICReq.ID
CDICReq.ID=CDICReq.Proyecto
CDICReq.Proyecto=CDICReq.mov
CDICReq.mov=CDICReq.movid
CDICReq.movid=CDICReq.origen
CDICReq.origen=CDICReq.origenid
CDICReq.origenid=CDICReq.FechaEmision
CDICReq.FechaEmision=CDICReq.usuario
CDICReq.usuario=CDICReq.moneda
CDICReq.moneda=CDICReq.TipoCambio
CDICReq.TipoCambio=CDICReq.Almacen
CDICReq.Almacen=CDICReq.Importe
CDICReq.Importe=CDICReq.impuestos
CDICReq.impuestos=CDICReq.FechaRegistro
CDICReq.FechaRegistro=CDICReq.FechaCancelacion
CDICReq.FechaCancelacion=CDICReq.FechaProveedor
CDICReq.FechaProveedor=CDICReq.FechaRequerida
CDICReq.FechaRequerida=CDICReq.CDIEstado
CDICReq.CDIEstado=CDICReq.Estatus
CDICReq.Estatus=CDICReq.Articulo
CDICReq.Articulo=CDICReq.Cantidad
CDICReq.Cantidad=CDICReq.CantidadA
CDICReq.CantidadA=CDICReq.CantidadPendiente
CDICReq.CantidadPendiente=CDICReq.Costo
CDICReq.Costo=CDICReq.Unidad
CDICReq.Unidad=CDICReq.FechaEntrega
CDICReq.FechaEntrega=CDICReq.Ejercicio
CDICReq.Ejercicio=CDICReq.periodo
CDICReq.periodo=CDICReq.Renglon
CDICReq.Renglon=CDICReq.Descripcion1
CDICReq.Descripcion1=CDICReq.ClavePresupuestal
CDICReq.ClavePresupuestal=CDICReq.Proveedor
CDICReq.Proveedor=CDICReq.Nombre
CDICReq.Nombre=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDICReq.ID,
CDICReq.ID,=CDICReq.Proyecto,
CDICReq.Proyecto,=CDICReq.mov,
CDICReq.mov,=CDICReq.movid,
CDICReq.movid,=CDICReq.origen,
CDICReq.origen,=CDICReq.origenid,
CDICReq.origenid,=CDICReq.FechaEmision,
CDICReq.FechaEmision,=CDICReq.usuario,
CDICReq.usuario,=CDICReq.moneda,
CDICReq.moneda,=CDICReq.TipoCambio,
CDICReq.TipoCambio,=CDICReq.Almacen,
CDICReq.Almacen,=CDICReq.Importe,
CDICReq.Importe,=CDICReq.impuestos,
CDICReq.impuestos,=CDICReq.FechaRegistro,
CDICReq.FechaRegistro,=CDICReq.FechaCancelacion,
CDICReq.FechaCancelacion,=CDICReq.FechaProveedor,
CDICReq.FechaProveedor,=CDICReq.FechaRequerida,
CDICReq.FechaRequerida,=CDICReq.CDIEstado,
CDICReq.CDIEstado,=CDICReq.Estatus,
CDICReq.Estatus,=CDICReq.Articulo,
CDICReq.Articulo,=CDICReq.Cantidad,
CDICReq.Cantidad,=CDICReq.CantidadA,
CDICReq.CantidadA,=CDICReq.CantidadPendiente,
CDICReq.CantidadPendiente,=CDICReq.Costo,
CDICReq.Costo,=CDICReq.Unidad,
CDICReq.Unidad,=CDICReq.FechaEntrega,
CDICReq.FechaEntrega,=CDICReq.Ejercicio,
CDICReq.Ejercicio,=CDICReq.periodo,
CDICReq.periodo,=CDICReq.Renglon,
CDICReq.Renglon,=CDICReq.Descripcion1,
CDICReq.Descripcion1,=CDICReq.ClavePresupuestal,
CDICReq.ClavePresupuestal,=CDICReq.Proveedor,
CDICReq.Proveedor,=CDICReq.Nombre
CDICReq.Nombre=FROM CDICReq
FROM CDICReq=(Fin)

[Vista.CamposBusquedaRapida]
(Inicio)=CDICReq.movid
CDICReq.movid=CDICReq.origenid
CDICReq.origenid=CDICReq.usuario
CDICReq.usuario=CDICReq.Almacen
CDICReq.Almacen=CDICReq.Articulo
CDICReq.Articulo=(Fin)
