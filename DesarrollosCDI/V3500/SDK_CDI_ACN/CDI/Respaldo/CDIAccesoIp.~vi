[Vista.SQL]
(Inicio)=SELECT
SELECT=CDIAccesoIp.Empresa,
CDIAccesoIp.Empresa,=CDIAccesoIp.Usuario,
CDIAccesoIp.Usuario,=CDIAccesoIp.FechaEmision,
CDIAccesoIp.FechaEmision,=CDIAccesoIp.Origen,
CDIAccesoIp.Origen,=CDIAccesoIp.Ip,
CDIAccesoIp.Ip,=CDIAccesoIp.Estatus
CDIAccesoIp.Estatus=FROM CDIAccesoIp
FROM CDIAccesoIp=(Fin)
