[Vista.ListaCampos]
(Inicio)=CDIVSlowEB1.ID
CDIVSlowEB1.ID=CDIVSlowEB1.ClaveTorneo
CDIVSlowEB1.ClaveTorneo=CDIVSlowEB1.NoVuelta
CDIVSlowEB1.NoVuelta=CDIVSlowEB1.NoJornada
CDIVSlowEB1.NoJornada=CDIVSlowEB1.fecha
CDIVSlowEB1.fecha=CDIVSlowEB1.Equipo
CDIVSlowEB1.Equipo=CDIVSlowEB1.jugador
CDIVSlowEB1.jugador=CDIVSlowEB1.Jugo
CDIVSlowEB1.Jugo=CDIVSlowEB1.VB
CDIVSlowEB1.VB=CDIVSlowEB1.C
CDIVSlowEB1.C=CDIVSlowEB1.H
CDIVSlowEB1.H=CDIVSlowEB1.h2
CDIVSlowEB1.h2=CDIVSlowEB1.h3
CDIVSlowEB1.h3=CDIVSlowEB1.HR
CDIVSlowEB1.HR=CDIVSlowEB1.CP
CDIVSlowEB1.CP=CDIVSlowEB1.K
CDIVSlowEB1.K=CDIVSlowEB1.BB
CDIVSlowEB1.BB=CDIVSlowEB1.GP
CDIVSlowEB1.GP=CDIVSlowEB1.SH
CDIVSlowEB1.SH=CDIVSlowEB1.SF
CDIVSlowEB1.SF=CDIVSlowEB1.br
CDIVSlowEB1.br=CDIVSlowEB1.Porcentaje
CDIVSlowEB1.Porcentaje=Cte.Cliente
Cte.Cliente=Cte.Nombre
Cte.Nombre=Cte.Socio
Cte.Socio=(Fin)

[Vista.ListaTablas]
(Inicio)=CDIVSlowEB1
CDIVSlowEB1=Cte
Cte=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDIVSlowEB1.ID,
CDIVSlowEB1.ID,=CDIVSlowEB1.ClaveTorneo,
CDIVSlowEB1.ClaveTorneo,=CDIVSlowEB1.NoVuelta,
CDIVSlowEB1.NoVuelta,=CDIVSlowEB1.NoJornada,
CDIVSlowEB1.NoJornada,=CDIVSlowEB1.fecha,
CDIVSlowEB1.fecha,=CDIVSlowEB1.Equipo,
CDIVSlowEB1.Equipo,=CDIVSlowEB1.jugador,
CDIVSlowEB1.jugador,=CDIVSlowEB1.Jugo,
CDIVSlowEB1.Jugo,=CDIVSlowEB1.VB,
CDIVSlowEB1.VB,=CDIVSlowEB1.C,
CDIVSlowEB1.C,=CDIVSlowEB1.H,
CDIVSlowEB1.H,=CDIVSlowEB1.h2,
CDIVSlowEB1.h2,=CDIVSlowEB1.h3,
CDIVSlowEB1.h3,=CDIVSlowEB1.HR,
CDIVSlowEB1.HR,=CDIVSlowEB1.CP,
CDIVSlowEB1.CP,=CDIVSlowEB1.K,
CDIVSlowEB1.K,=CDIVSlowEB1.BB,
CDIVSlowEB1.BB,=CDIVSlowEB1.GP,
CDIVSlowEB1.GP,=CDIVSlowEB1.SH,
CDIVSlowEB1.SH,=CDIVSlowEB1.SF,
CDIVSlowEB1.SF,=CDIVSlowEB1.br,
CDIVSlowEB1.br,=CDIVSlowEB1.Porcentaje,
CDIVSlowEB1.Porcentaje,=Cte.Cliente,
Cte.Cliente,=Cte.Nombre,
Cte.Nombre,=Cte.Socio
Cte.Socio=FROM CDIVSlowEB1
FROM CDIVSlowEB1=JOIN Cte ON CDIVSlowEB1.jugador<IGUAL>Cte.Socio
JOIN Cte ON CDIVSlowEB1.jugador<IGUAL>Cte.Socio=(Fin)
