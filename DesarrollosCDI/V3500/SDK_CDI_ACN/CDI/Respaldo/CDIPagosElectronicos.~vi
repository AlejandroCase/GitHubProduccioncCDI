[Vista.ListaCampos]
(Inicio)=CDIPagosElectronicos.id
CDIPagosElectronicos.id=CDIPagosElectronicos.Mov
CDIPagosElectronicos.Mov=CDIPagosElectronicos.MovId
CDIPagosElectronicos.MovId=CDIPagosElectronicos.FechaEmision
CDIPagosElectronicos.FechaEmision=CDIPagosElectronicos.Proyecto
CDIPagosElectronicos.Proyecto=CDIPagosElectronicos.Referencia
CDIPagosElectronicos.Referencia=CDIPagosElectronicos.BeneficiarioNombre
CDIPagosElectronicos.BeneficiarioNombre=CDIPagosElectronicos.Contacto
CDIPagosElectronicos.Contacto=CDIPagosElectronicos.ContactoTipo
CDIPagosElectronicos.ContactoTipo=CDIPagosElectronicos.Importe
CDIPagosElectronicos.Importe=CDIPagosElectronicos.Redondeo20
CDIPagosElectronicos.Redondeo20=CDIPagosElectronicos.EstaImpreso
CDIPagosElectronicos.EstaImpreso=CDIPagosElectronicos.CtaDinero
CDIPagosElectronicos.CtaDinero=CDIPagosElectronicos.NumeroCta
CDIPagosElectronicos.NumeroCta=CDIPagosElectronicos.FechaProgramada
CDIPagosElectronicos.FechaProgramada=CDIPagosElectronicos.Proveedor
CDIPagosElectronicos.Proveedor=CDIPagosElectronicos.Nombre
CDIPagosElectronicos.Nombre=CDIPagosElectronicos.ProvBancoSucursal
CDIPagosElectronicos.ProvBancoSucursal=CDIPagosElectronicos.ProvCuenta
CDIPagosElectronicos.ProvCuenta=CDIPagosElectronicos.RFC
CDIPagosElectronicos.RFC=CDIPagosElectronicos.Email1
CDIPagosElectronicos.Email1=CDIPagosElectronicos.Moneda
CDIPagosElectronicos.Moneda=CDIPagosElectronicos.NombreCorto
CDIPagosElectronicos.NombreCorto=CDIPagosElectronicos.ProvBanco
CDIPagosElectronicos.ProvBanco=CDIPagosElectronicos.CDIBanco
CDIPagosElectronicos.CDIBanco=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDIPagosElectronicos.id,
CDIPagosElectronicos.id,=CDIPagosElectronicos.Mov,
CDIPagosElectronicos.Mov,=CDIPagosElectronicos.MovId,
CDIPagosElectronicos.MovId,=CDIPagosElectronicos.FechaEmision,
CDIPagosElectronicos.FechaEmision,=CDIPagosElectronicos.Proyecto,
CDIPagosElectronicos.Proyecto,=CDIPagosElectronicos.Referencia,
CDIPagosElectronicos.Referencia,=CDIPagosElectronicos.BeneficiarioNombre,
CDIPagosElectronicos.BeneficiarioNombre,=CDIPagosElectronicos.Contacto,
CDIPagosElectronicos.Contacto,=CDIPagosElectronicos.ContactoTipo,
CDIPagosElectronicos.ContactoTipo,=CDIPagosElectronicos.Importe,
CDIPagosElectronicos.Importe,=CDIPagosElectronicos.Redondeo20,
CDIPagosElectronicos.Redondeo20,=CDIPagosElectronicos.EstaImpreso,
CDIPagosElectronicos.EstaImpreso,=CDIPagosElectronicos.CtaDinero,
CDIPagosElectronicos.CtaDinero,=CDIPagosElectronicos.NumeroCta,
CDIPagosElectronicos.NumeroCta,=CDIPagosElectronicos.FechaProgramada,
CDIPagosElectronicos.FechaProgramada,=CDIPagosElectronicos.Proveedor,
CDIPagosElectronicos.Proveedor,=CDIPagosElectronicos.Nombre,
CDIPagosElectronicos.Nombre,=CDIPagosElectronicos.ProvBancoSucursal,
CDIPagosElectronicos.ProvBancoSucursal,=CDIPagosElectronicos.ProvCuenta,
CDIPagosElectronicos.ProvCuenta,=CDIPagosElectronicos.RFC,
CDIPagosElectronicos.RFC,=CDIPagosElectronicos.Email1,
CDIPagosElectronicos.Email1,=CDIPagosElectronicos.Moneda,
CDIPagosElectronicos.Moneda,=CDIPagosElectronicos.NombreCorto,
CDIPagosElectronicos.NombreCorto,=CDIPagosElectronicos.ProvBanco,
CDIPagosElectronicos.ProvBanco,=CDIPagosElectronicos.CDIBanco
CDIPagosElectronicos.CDIBanco=FROM CDIPagosElectronicos
FROM CDIPagosElectronicos=(Fin)

[Vista.CamposBusquedaRapida]
(Inicio)=CDIPagosElectronicos.Mov
CDIPagosElectronicos.Mov=CDIPagosElectronicos.MovId
CDIPagosElectronicos.MovId=CDIPagosElectronicos.NombreCorto
CDIPagosElectronicos.NombreCorto=CDIPagosElectronicos.Proveedor
CDIPagosElectronicos.Proveedor=CDIPagosElectronicos.Nombre
CDIPagosElectronicos.Nombre=CDIPagosElectronicos.RFC
CDIPagosElectronicos.RFC=(Fin)
