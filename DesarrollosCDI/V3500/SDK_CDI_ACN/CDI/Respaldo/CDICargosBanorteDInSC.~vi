[Vista.ListaCampos]
(Inicio)=CDICargosBanorteDInSC.Id
CDICargosBanorteDInSC.Id=CDICargosBanorteDInSC.Renglon
CDICargosBanorteDInSC.Renglon=CDICargosBanorteDInSC.Referencia
CDICargosBanorteDInSC.Referencia=CDICargosBanorteDInSC.NUMERO_CONTROL
CDICargosBanorteDInSC.NUMERO_CONTROL=CDICargosBanorteDInSC.NUMERO_TARJETA
CDICargosBanorteDInSC.NUMERO_TARJETA=CDICargosBanorteDInSC.FECHA_EXP
CDICargosBanorteDInSC.FECHA_EXP=CDICargosBanorteDInSC.MONTO
CDICargosBanorteDInSC.MONTO=CDICargosBanorteDInSC.ESTATUS
CDICargosBanorteDInSC.ESTATUS=CDICargosBanorteDInSC.CodAutorizacion
CDICargosBanorteDInSC.CodAutorizacion=CDICargosBanorteDInSC.MensajeRespuesta
CDICargosBanorteDInSC.MensajeRespuesta=CDICargosBanorteDInSC.CodRespuesta
CDICargosBanorteDInSC.CodRespuesta=CDICargosBanorteDInSC.Programada
CDICargosBanorteDInSC.Programada=CDICargosBanorteDInSC.Comentarios
CDICargosBanorteDInSC.Comentarios=CDICargosBanorteDInSC.EstatusProc
CDICargosBanorteDInSC.EstatusProc=CDICargosBanorteDInSC.IdCxcCob
CDICargosBanorteDInSC.IdCxcCob=CDICargosBanorteDInSC.IDRecCxc
CDICargosBanorteDInSC.IDRecCxc=CDICargosBanorteDInSC.IDRecVta
CDICargosBanorteDInSC.IDRecVta=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDICargosBanorteDInSC.Id,
CDICargosBanorteDInSC.Id,=CDICargosBanorteDInSC.Renglon,
CDICargosBanorteDInSC.Renglon,=CDICargosBanorteDInSC.Referencia,
CDICargosBanorteDInSC.Referencia,=CDICargosBanorteDInSC.NUMERO_CONTROL,
CDICargosBanorteDInSC.NUMERO_CONTROL,=CDICargosBanorteDInSC.NUMERO_TARJETA,
CDICargosBanorteDInSC.NUMERO_TARJETA,=CDICargosBanorteDInSC.FECHA_EXP,
CDICargosBanorteDInSC.FECHA_EXP,=CDICargosBanorteDInSC.MONTO,
CDICargosBanorteDInSC.MONTO,=CDICargosBanorteDInSC.ESTATUS,
CDICargosBanorteDInSC.ESTATUS,=CDICargosBanorteDInSC.CodAutorizacion,
CDICargosBanorteDInSC.CodAutorizacion,=CDICargosBanorteDInSC.MensajeRespuesta,
CDICargosBanorteDInSC.MensajeRespuesta,=CDICargosBanorteDInSC.CodRespuesta,
CDICargosBanorteDInSC.CodRespuesta,=CDICargosBanorteDInSC.Programada,
CDICargosBanorteDInSC.Programada,=CDICargosBanorteDInSC.Comentarios,
CDICargosBanorteDInSC.Comentarios,=CDICargosBanorteDInSC.EstatusProc,
CDICargosBanorteDInSC.EstatusProc,=CDICargosBanorteDInSC.IdCxcCob,
CDICargosBanorteDInSC.IdCxcCob,=CDICargosBanorteDInSC.IDRecCxc,
CDICargosBanorteDInSC.IDRecCxc,=CDICargosBanorteDInSC.IDRecVta
CDICargosBanorteDInSC.IDRecVta=FROM CDICargosBanorteDInSC
FROM CDICargosBanorteDInSC=(Fin)

