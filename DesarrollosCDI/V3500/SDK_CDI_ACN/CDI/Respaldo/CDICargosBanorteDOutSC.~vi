[Vista.ListaCampos]
(Inicio)=CDICargosBanorteDOutSC.Id
CDICargosBanorteDOutSC.Id=CDICargosBanorteDOutSC.Renglon
CDICargosBanorteDOutSC.Renglon=CDICargosBanorteDOutSC.CMD_TRANS
CDICargosBanorteDOutSC.CMD_TRANS=CDICargosBanorteDOutSC.MONTO
CDICargosBanorteDOutSC.MONTO=CDICargosBanorteDOutSC.COMENTARIOS
CDICargosBanorteDOutSC.COMENTARIOS=CDICargosBanorteDOutSC.LOTE
CDICargosBanorteDOutSC.LOTE=CDICargosBanorteDOutSC.NUMERO_CONTROL
CDICargosBanorteDOutSC.NUMERO_CONTROL=CDICargosBanorteDOutSC.NUMERO_CONTRATO
CDICargosBanorteDOutSC.NUMERO_CONTRATO=CDICargosBanorteDOutSC.REFERENCIA_PROSA
CDICargosBanorteDOutSC.REFERENCIA_PROSA=CDICargosBanorteDOutSC.NUMERO_TARJETA
CDICargosBanorteDOutSC.NUMERO_TARJETA=CDICargosBanorteDOutSC.FECHA_EXP
CDICargosBanorteDOutSC.FECHA_EXP=CDICargosBanorteDOutSC.Cliente
CDICargosBanorteDOutSC.Cliente=CDICargosBanorteDOutSC.Socio
CDICargosBanorteDOutSC.Socio=CDICargosBanorteDOutSC.ModuloId
CDICargosBanorteDOutSC.ModuloId=CDICargosBanorteDOutSC.Mov
CDICargosBanorteDOutSC.Mov=CDICargosBanorteDOutSC.MovId
CDICargosBanorteDOutSC.MovId=CDICargosBanorteDOutSC.Importe
CDICargosBanorteDOutSC.Importe=CDICargosBanorteDOutSC.EstatusProcesar
CDICargosBanorteDOutSC.EstatusProcesar=CDICargosBanorteDOutSC.Procesar
CDICargosBanorteDOutSC.Procesar=CDICargosBanorteDOutSC.MovCxC
CDICargosBanorteDOutSC.MovCxC=CDICargosBanorteDOutSC.MovIdCxC
CDICargosBanorteDOutSC.MovIdCxC=Cte.Nombre
Cte.Nombre=Cte.Telefonos
Cte.Telefonos=(Fin)

[Vista.ListaTablas]
(Inicio)=CDICargosBanorteDOutSC
CDICargosBanorteDOutSC=Cte
Cte=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDICargosBanorteDOutSC.Id,
CDICargosBanorteDOutSC.Id,=CDICargosBanorteDOutSC.Renglon,
CDICargosBanorteDOutSC.Renglon,=CDICargosBanorteDOutSC.CMD_TRANS,
CDICargosBanorteDOutSC.CMD_TRANS,=CDICargosBanorteDOutSC.MONTO,
CDICargosBanorteDOutSC.MONTO,=CDICargosBanorteDOutSC.COMENTARIOS,
CDICargosBanorteDOutSC.COMENTARIOS,=CDICargosBanorteDOutSC.LOTE,
CDICargosBanorteDOutSC.LOTE,=CDICargosBanorteDOutSC.NUMERO_CONTROL,
CDICargosBanorteDOutSC.NUMERO_CONTROL,=CDICargosBanorteDOutSC.NUMERO_CONTRATO,
CDICargosBanorteDOutSC.NUMERO_CONTRATO,=CDICargosBanorteDOutSC.REFERENCIA_PROSA,
CDICargosBanorteDOutSC.REFERENCIA_PROSA,=CDICargosBanorteDOutSC.NUMERO_TARJETA,
CDICargosBanorteDOutSC.NUMERO_TARJETA,=CDICargosBanorteDOutSC.FECHA_EXP,
CDICargosBanorteDOutSC.FECHA_EXP,=CDICargosBanorteDOutSC.Cliente,
CDICargosBanorteDOutSC.Cliente,=CDICargosBanorteDOutSC.Socio,
CDICargosBanorteDOutSC.Socio,=CDICargosBanorteDOutSC.ModuloId,
CDICargosBanorteDOutSC.ModuloId,=CDICargosBanorteDOutSC.Mov,
CDICargosBanorteDOutSC.Mov,=CDICargosBanorteDOutSC.MovId,
CDICargosBanorteDOutSC.MovId,=CDICargosBanorteDOutSC.Importe,
CDICargosBanorteDOutSC.Importe,=CDICargosBanorteDOutSC.EstatusProcesar,
CDICargosBanorteDOutSC.EstatusProcesar,=CDICargosBanorteDOutSC.Procesar,
CDICargosBanorteDOutSC.Procesar,=CDICargosBanorteDOutSC.MovCxC,
CDICargosBanorteDOutSC.MovCxC,=CDICargosBanorteDOutSC.MovIdCxC,
CDICargosBanorteDOutSC.MovIdCxC,=Cte.Nombre,
Cte.Nombre,=Cte.Telefonos
Cte.Telefonos=FROM CDICargosBanorteDOutSC
FROM CDICargosBanorteDOutSC=JOIN Cte ON CDICargosBanorteDOutSC.Socio<IGUAL>Cte.Socio
JOIN Cte ON CDICargosBanorteDOutSC.Socio<IGUAL>Cte.Socio=(Fin)

[Vista.CamposBusquedaRapida]
(Inicio)=CDICargosBanorteDOutSC.NUMERO_TARJETA
CDICargosBanorteDOutSC.NUMERO_TARJETA=CDICargosBanorteDOutSC.Socio
CDICargosBanorteDOutSC.Socio=CDICargosBanorteDOutSC.Cliente
CDICargosBanorteDOutSC.Cliente=CDICargosBanorteDOutSC.MovId
CDICargosBanorteDOutSC.MovId=CDICargosBanorteDOutSC.MovIdCxC
CDICargosBanorteDOutSC.MovIdCxC=Cte.Nombre
Cte.Nombre=(Fin)
