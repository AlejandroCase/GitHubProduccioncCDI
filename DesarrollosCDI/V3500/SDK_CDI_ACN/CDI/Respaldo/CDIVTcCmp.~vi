[Vista.SQL]
(Inicio)=SELECT
SELECT=CDIVTcCmp.id,
CDIVTcCmp.id,=CDIVTcCmp.mov,
CDIVTcCmp.mov,=CDIVTcCmp.movid,
CDIVTcCmp.movid,=CDIVTcCmp.IdCmp,
CDIVTcCmp.IdCmp,=CDIVTcCmp.TcCobro,
CDIVTcCmp.TcCobro,=CDIVTcCmp.CsCobro
CDIVTcCmp.CsCobro=FROM CDIVTcCmp
FROM CDIVTcCmp=(Fin)
