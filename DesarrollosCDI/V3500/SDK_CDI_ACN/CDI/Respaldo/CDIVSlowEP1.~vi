[Vista.ListaCampos]
(Inicio)=CDIVSlowEP1.ID
CDIVSlowEP1.ID=CDIVSlowEP1.ClaveTorneo
CDIVSlowEP1.ClaveTorneo=CDIVSlowEP1.NoVuelta
CDIVSlowEP1.NoVuelta=CDIVSlowEP1.NoJornada
CDIVSlowEP1.NoJornada=CDIVSlowEP1.fecha
CDIVSlowEP1.fecha=CDIVSlowEP1.Equipo
CDIVSlowEP1.Equipo=CDIVSlowEP1.jugador
CDIVSlowEP1.jugador=CDIVSlowEP1.PCLA
CDIVSlowEP1.PCLA=CDIVSlowEP1.jugo
CDIVSlowEP1.jugo=CDIVSlowEP1.gano
CDIVSlowEP1.gano=CDIVSlowEP1.Perdio
CDIVSlowEP1.Perdio=CDIVSlowEP1.IP
CDIVSlowEP1.IP=CDIVSlowEP1.VBC
CDIVSlowEP1.VBC=CDIVSlowEP1.HA
CDIVSlowEP1.HA=CDIVSlowEP1.HR
CDIVSlowEP1.HR=CDIVSlowEP1.CA
CDIVSlowEP1.CA=CDIVSlowEP1.CL
CDIVSlowEP1.CL=CDIVSlowEP1.BB
CDIVSlowEP1.BB=CDIVSlowEP1.SO
CDIVSlowEP1.SO=Cte.Cliente
Cte.Cliente=Cte.Nombre
Cte.Nombre=Cte.Socio
Cte.Socio=(Fin)

[Vista.ListaTablas]
(Inicio)=CDIVSlowEP1
CDIVSlowEP1=Cte
Cte=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDIVSlowEP1.ID,
CDIVSlowEP1.ID,=CDIVSlowEP1.ClaveTorneo,
CDIVSlowEP1.ClaveTorneo,=CDIVSlowEP1.NoVuelta,
CDIVSlowEP1.NoVuelta,=CDIVSlowEP1.NoJornada,
CDIVSlowEP1.NoJornada,=CDIVSlowEP1.fecha,
CDIVSlowEP1.fecha,=CDIVSlowEP1.Equipo,
CDIVSlowEP1.Equipo,=CDIVSlowEP1.jugador,
CDIVSlowEP1.jugador,=CDIVSlowEP1.PCLA,
CDIVSlowEP1.PCLA,=CDIVSlowEP1.jugo,
CDIVSlowEP1.jugo,=CDIVSlowEP1.gano,
CDIVSlowEP1.gano,=CDIVSlowEP1.Perdio,
CDIVSlowEP1.Perdio,=CDIVSlowEP1.IP,
CDIVSlowEP1.IP,=CDIVSlowEP1.VBC,
CDIVSlowEP1.VBC,=CDIVSlowEP1.HA,
CDIVSlowEP1.HA,=CDIVSlowEP1.HR,
CDIVSlowEP1.HR,=CDIVSlowEP1.CA,
CDIVSlowEP1.CA,=CDIVSlowEP1.CL,
CDIVSlowEP1.CL,=CDIVSlowEP1.BB,
CDIVSlowEP1.BB,=CDIVSlowEP1.SO,
CDIVSlowEP1.SO,=Cte.Cliente,
Cte.Cliente,=Cte.Nombre,
Cte.Nombre,=Cte.Socio
Cte.Socio=FROM CDIVSlowEP1
FROM CDIVSlowEP1=JOIN Cte ON CDIVSlowEP1.jugador<IGUAL>Cte.Socio
JOIN Cte ON CDIVSlowEP1.jugador<IGUAL>Cte.Socio=(Fin)
