[Vista.ListaCampos]
(Inicio)=CDIExpAsistePers.Id
CDIExpAsistePers.Id=CDIExpAsistePers.Empresa
CDIExpAsistePers.Empresa=CDIExpAsistePers.Mov
CDIExpAsistePers.Mov=CDIExpAsistePers.FechaEmision
CDIExpAsistePers.FechaEmision=CDIExpAsistePers.Usuario
CDIExpAsistePers.Usuario=CDIExpAsistePers.Estatus
CDIExpAsistePers.Estatus=CDIExpAsistePers.Nombre
CDIExpAsistePers.Nombre=CDIExpAsistePers.ApellidoPaterno
CDIExpAsistePers.ApellidoPaterno=CDIExpAsistePers.ApellidoMaterno
CDIExpAsistePers.ApellidoMaterno=CDIExpAsistePers.Tipo
CDIExpAsistePers.Tipo=CDIExpAsistePers.Area
CDIExpAsistePers.Area=CDIExpAsistePers.Departamento
CDIExpAsistePers.Departamento=CDIExpAsistePers.Puesto
CDIExpAsistePers.Puesto=CDIExpAsistePers.Categoria
CDIExpAsistePers.Categoria=CDIExpAsistePers.PeriodoTipo
CDIExpAsistePers.PeriodoTipo=CDIExpAsistePers.Jornada
CDIExpAsistePers.Jornada=CDIExpAsistePers.Proyecto
CDIExpAsistePers.Proyecto=CDIExpAsistePers.EstatusPersonal
CDIExpAsistePers.EstatusPersonal=CDIExpAsistePers.Ejercicio
CDIExpAsistePers.Ejercicio=CDIExpAsistePers.Periodo
CDIExpAsistePers.Periodo=CDIExpAsistePers.PersonalEntrada
CDIExpAsistePers.PersonalEntrada=CDIExpAsistePers.RegistroEntrada
CDIExpAsistePers.RegistroEntrada=CDIExpAsistePers.HoraEntrada
CDIExpAsistePers.HoraEntrada=CDIExpAsistePers.FechaEntrada
CDIExpAsistePers.FechaEntrada=CDIExpAsistePers.PersonalSalida
CDIExpAsistePers.PersonalSalida=CDIExpAsistePers.RegistroSalida
CDIExpAsistePers.RegistroSalida=CDIExpAsistePers.HoraSalida
CDIExpAsistePers.HoraSalida=CDIExpAsistePers.FechaSalida
CDIExpAsistePers.FechaSalida=CDIExpAsistePers.DiaLaboral
CDIExpAsistePers.DiaLaboral=Personal.CDIPerImagen
Personal.CDIPerImagen=(Fin)

[Vista.ListaTablas]
(Inicio)=CDIExpAsistePers
CDIExpAsistePers=Personal
Personal=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDIExpAsistePers.Id,
CDIExpAsistePers.Id,=CDIExpAsistePers.Empresa,
CDIExpAsistePers.Empresa,=CDIExpAsistePers.Mov,
CDIExpAsistePers.Mov,=CDIExpAsistePers.FechaEmision,
CDIExpAsistePers.FechaEmision,=CDIExpAsistePers.Usuario,
CDIExpAsistePers.Usuario,=CDIExpAsistePers.Estatus,
CDIExpAsistePers.Estatus,=CDIExpAsistePers.Nombre,
CDIExpAsistePers.Nombre,=CDIExpAsistePers.ApellidoPaterno,
CDIExpAsistePers.ApellidoPaterno,=CDIExpAsistePers.ApellidoMaterno,
CDIExpAsistePers.ApellidoMaterno,=CDIExpAsistePers.Tipo,
CDIExpAsistePers.Tipo,=CDIExpAsistePers.Area,
CDIExpAsistePers.Area,=CDIExpAsistePers.Departamento,
CDIExpAsistePers.Departamento,=CDIExpAsistePers.Puesto,
CDIExpAsistePers.Puesto,=CDIExpAsistePers.Categoria,
CDIExpAsistePers.Categoria,=CDIExpAsistePers.PeriodoTipo,
CDIExpAsistePers.PeriodoTipo,=CDIExpAsistePers.Jornada,
CDIExpAsistePers.Jornada,=CDIExpAsistePers.Proyecto,
CDIExpAsistePers.Proyecto,=CDIExpAsistePers.EstatusPersonal,
CDIExpAsistePers.EstatusPersonal,=CDIExpAsistePers.Ejercicio,
CDIExpAsistePers.Ejercicio,=CDIExpAsistePers.Periodo,
CDIExpAsistePers.Periodo,=CDIExpAsistePers.PersonalEntrada,
CDIExpAsistePers.PersonalEntrada,=CDIExpAsistePers.RegistroEntrada,
CDIExpAsistePers.RegistroEntrada,=CDIExpAsistePers.HoraEntrada,
CDIExpAsistePers.HoraEntrada,=CDIExpAsistePers.FechaEntrada,
CDIExpAsistePers.FechaEntrada,=CDIExpAsistePers.PersonalSalida,
CDIExpAsistePers.PersonalSalida,=CDIExpAsistePers.RegistroSalida,
CDIExpAsistePers.RegistroSalida,=CDIExpAsistePers.HoraSalida,
CDIExpAsistePers.HoraSalida,=CDIExpAsistePers.FechaSalida,
CDIExpAsistePers.FechaSalida,=CDIExpAsistePers.DiaLaboral,
CDIExpAsistePers.DiaLaboral,=Personal.CDIPerImagen
Personal.CDIPerImagen=FROM CDIExpAsistePers
FROM CDIExpAsistePers=JOIN Personal ON CDIExpAsistePers.PersonalEntrada<IGUAL>Personal.Personal
JOIN Personal ON CDIExpAsistePers.PersonalEntrada<IGUAL>Personal.Personal=(Fin)

[Vista.CamposBusquedaRapida]
(Inicio)=CDIExpAsistePers.Id
CDIExpAsistePers.Id=CDIExpAsistePers.PersonalEntrada
CDIExpAsistePers.PersonalEntrada=CDIExpAsistePers.PersonalSalida
CDIExpAsistePers.PersonalSalida=(Fin)
