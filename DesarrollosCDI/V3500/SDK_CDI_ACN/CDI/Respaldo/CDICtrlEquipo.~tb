[Tabla.SQL]
(Inicio)=SELECT
SELECT=ID,
ID,=Mov,
Mov,=Movid,
Movid,=FechaEmision,
FechaEmision,=Deporte,
Deporte,=RamaDepor,
RamaDepor,=Fuerza,
Fuerza,=TorneoID,
TorneoID,=Nombre,
Nombre,=CapitanSocio,
CapitanSocio,=SubCapitanSocio,
SubCapitanSocio,=PreferenciaDias,
PreferenciaDias,=PreferenciaHorario,
PreferenciaHorario,=Religioso,
Religioso,=Estatus,
Estatus,=Situacion,
Situacion,=SituacionFecha,
SituacionFecha,=SituacionUsuario,
SituacionUsuario,=Usuario,
Usuario,=Empresa,
Empresa,=Proyecto,
Proyecto,=RenglonId,
RenglonId,=Clave,
Clave,=ClaveEqJug,
ClaveEqJug,=Filial,
Filial,=EquipoFilial,
EquipoFilial,=EstatusEquipo,
EstatusEquipo,=Externo,
Externo,=Representativo
Representativo=FROM CDICtrlEquipo
FROM CDICtrlEquipo=(Fin)
