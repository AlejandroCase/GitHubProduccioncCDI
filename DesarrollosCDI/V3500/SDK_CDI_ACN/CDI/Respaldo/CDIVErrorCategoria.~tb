[Tabla.ListaCampos]
(Inicio)=Tipo
Tipo=Categoria
Categoria=SubCategoria
SubCategoria=Aporta
Aporta=Clase
Clase=Actividad
Actividad=Sede
Sede=Titular
Titular=Estatus
Estatus=Cliente
Cliente=Membresia
Membresia=Socio
Socio=Nombre
Nombre=Email1
Email1=Email2
Email2=edad
edad=total
total=menores3
menores3=Juveniles
Juveniles=menores68
menores68=mayores68NoTitulares
mayores68NoTitulares=mayores68Titulares
mayores68Titulares=EstatusCat
EstatusCat=EstatusSubCat
EstatusSubCat=EstatusCargoRecurrente
EstatusCargoRecurrente=CategoriaCorrecta
CategoriaCorrecta=SubCategoriaCorrecta
SubCategoriaCorrecta=CargoRecurrenteCorrecto
CargoRecurrenteCorrecto=CargoRecurrenteSocio
CargoRecurrenteSocio=(Fin)

[Tabla.SQL]
(Inicio)=SELECT
SELECT=Tipo,
Tipo,=Categoria,
Categoria,=SubCategoria,
SubCategoria,=Aporta,
Aporta,=Clase,
Clase,=Actividad,
Actividad,=Sede,
Sede,=Titular,
Titular,=Estatus,
Estatus,=Cliente,
Cliente,=Membresia,
Membresia,=Socio,
Socio,=Nombre,
Nombre,=Email1,
Email1,=Email2,
Email2,=edad,
edad,=total,
total,=menores3,
menores3,=Juveniles,
Juveniles,=menores68,
menores68,=mayores68NoTitulares,
mayores68NoTitulares,=mayores68Titulares,
mayores68Titulares,=CategoriaCorrecta,
CategoriaCorrecta,=EstatusCat,
EstatusCat,=SubCategoriaCorrecta,
SubCategoriaCorrecta,=EstatusSubCat,
EstatusSubCat,=CargoRecurrenteCorrecto,
CargoRecurrenteCorrecto,=CargoRecurrenteSocio,
CargoRecurrenteSocio,=EstatusCargoRecurrente
EstatusCargoRecurrente=FROM CDIVErrorCategoria
FROM CDIVErrorCategoria=(Fin)
