[Tabla.ListaCampos]
(Inicio)=id
id=mov
mov=movid
movid=articulo
articulo=precio
precio=PrecioSugerido
PrecioSugerido=DescuentoTipo
DescuentoTipo=DescuentoLinea
DescuentoLinea=DescuentoImporte
DescuentoImporte=Impuesto1
Impuesto1=Impuesto2
Impuesto2=CDIClavePresupuestal
CDIClavePresupuestal=Aplica
Aplica=AplicaID
AplicaID=PrecioCDescuento
PrecioCDescuento=ImpuestoPrecioCDescuento
ImpuestoPrecioCDescuento=PrecioNeto
PrecioNeto=(Fin)

[Tabla.SQL]
(Inicio)=SELECT
SELECT=id,
id,=mov,
mov,=movid,
movid,=articulo,
articulo,=precio,
precio,=PrecioSugerido,
PrecioSugerido,=DescuentoTipo,
DescuentoTipo,=DescuentoLinea,
DescuentoLinea,=DescuentoImporte,
DescuentoImporte,=Impuesto1,
Impuesto1,=Impuesto2,
Impuesto2,=CDIClavePresupuestal,
CDIClavePresupuestal,=Aplica,
Aplica,=AplicaID,
AplicaID,=PrecioCDescuento,
PrecioCDescuento,=ImpuestoPrecioCDescuento,
ImpuestoPrecioCDescuento,=PrecioNeto
PrecioNeto=FROM CDIVentaDtodo
FROM CDIVentaDtodo=(Fin)
