[Vista.ListaCampos]
(Inicio)=CDIInscGrupoD.Id
CDIInscGrupoD.Id=CDIInscGrupoD.Renglon
CDIInscGrupoD.Renglon=CDIInscGrupoD.IdGrupo
CDIInscGrupoD.IdGrupo=CDIInscGrupoD.Socio
CDIInscGrupoD.Socio=CDIInscGrupoD.Membresia
CDIInscGrupoD.Membresia=CDIInscGrupoD.IdPreRegistro
CDIInscGrupoD.IdPreRegistro=CDIInscGrupoD.IdSolCargo
CDIInscGrupoD.IdSolCargo=CDIInscGrupoD.IdRecibo
CDIInscGrupoD.IdRecibo=CDIInscGrupoD.IdCobro
CDIInscGrupoD.IdCobro=CDIInscGrupoD.Estatus
CDIInscGrupoD.Estatus=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDIInscGrupoD.Id,
CDIInscGrupoD.Id,=CDIInscGrupoD.Renglon,
CDIInscGrupoD.Renglon,=CDIInscGrupoD.IdGrupo,
CDIInscGrupoD.IdGrupo,=CDIInscGrupoD.Socio,
CDIInscGrupoD.Socio,=CDIInscGrupoD.Membresia,
CDIInscGrupoD.Membresia,=CDIInscGrupoD.IdPreRegistro,
CDIInscGrupoD.IdPreRegistro,=CDIInscGrupoD.IdSolCargo,
CDIInscGrupoD.IdSolCargo,=CDIInscGrupoD.IdRecibo,
CDIInscGrupoD.IdRecibo,=CDIInscGrupoD.IdCobro,
CDIInscGrupoD.IdCobro,=CDIInscGrupoD.Estatus
CDIInscGrupoD.Estatus=FROM CDIInscGrupoD
FROM CDIInscGrupoD=(Fin)
