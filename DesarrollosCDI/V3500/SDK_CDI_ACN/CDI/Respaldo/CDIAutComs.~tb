[Tabla.ListaCampos]
(Inicio)=ID
ID=Proyecto
Proyecto=mov
mov=movid
movid=origen
origen=origenid
origenid=FechaEmision
FechaEmision=usuario
usuario=moneda
moneda=TipoCambio
TipoCambio=Almacen
Almacen=Importe
Importe=impuestos
impuestos=FechaRegistro
FechaRegistro=FechaCancelacion
FechaCancelacion=FechaProveedor
FechaProveedor=FechaRequerida
FechaRequerida=CDIEstado
CDIEstado=Estatus
Estatus=Ejercicio
Ejercicio=periodo
periodo=Proveedor
Proveedor=Nombre
Nombre=Articulo
Articulo=Cantidad
Cantidad=CantidadA
CantidadA=CantidadPendiente
CantidadPendiente=Costo
Costo=Unidad
Unidad=FechaEntrega
FechaEntrega=Renglon
Renglon=Descripcion1
Descripcion1=ClavePresupuestal
ClavePresupuestal=situacion
situacion=situacionfecha
situacionfecha=situacionusuario
situacionusuario=Estacion
Estacion=SitAnterior
SitAnterior=(Fin)

[Tabla.SQL]
(Inicio)=SELECT
SELECT=Proyecto,
Proyecto,=mov,
mov,=movid,
movid,=origen,
origen,=origenid,
origenid,=FechaEmision,
FechaEmision,=usuario,
usuario,=moneda,
moneda,=TipoCambio,
TipoCambio,=Almacen,
Almacen,=Importe,
Importe,=impuestos,
impuestos,=FechaRegistro,
FechaRegistro,=FechaCancelacion,
FechaCancelacion,=FechaProveedor,
FechaProveedor,=FechaRequerida,
FechaRequerida,=CDIEstado,
CDIEstado,=Estatus,
Estatus,=Ejercicio,
Ejercicio,=periodo,
periodo,=Proveedor,
Proveedor,=Nombre,
Nombre,=Articulo,
Articulo,=Cantidad,
Cantidad,=CantidadA,
CantidadA,=CantidadPendiente,
CantidadPendiente,=Costo,
Costo,=Unidad,
Unidad,=FechaEntrega,
FechaEntrega,=Renglon,
Renglon,=Descripcion1,
Descripcion1,=ClavePresupuestal,
ClavePresupuestal,=situacion,
situacion,=situacionfecha,
situacionfecha,=situacionusuario,
situacionusuario,=Estacion,
Estacion,=id,
id,=SitAnterior
SitAnterior=FROM CDIAutComs
FROM CDIAutComs=(Fin)
