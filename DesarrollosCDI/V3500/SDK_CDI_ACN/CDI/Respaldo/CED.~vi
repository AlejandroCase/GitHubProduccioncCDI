[Importe]
Clave=Importe
TipoDatos=NumFlotante
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Importe
Monetario=S
CondicionDespliege=MovTipoEn(<T>CE<T>, CE:CE.Mov, (CE.P, CE.I))
Formula=ImporteNeto-DescLinea
