[Tabla.ListaCampos]
(Inicio)=id
id=Cobro
Cobro=ConsecCobro
ConsecCobro=Cliente
Cliente=Nombre
Nombre=Importe
Importe=Impuestos
Impuestos=Recibo
Recibo=ConsecRec
ConsecRec=Articulo
Articulo=Precio
Precio=DescuentoLinea
DescuentoLinea=DescuentoImporte
DescuentoImporte=SolCargo
SolCargo=ConsecSolCargo
ConsecSolCargo=PreRegistro
PreRegistro=ConsecPre
ConsecPre=CDIclavepresupuestal
CDIclavepresupuestal=Descripcion
Descripcion=Curso
Curso=Proyecto
Proyecto=Ejercicio
Ejercicio=Periodo
Periodo=Subsidio
Subsidio=Orden
Orden=cantidad
cantidad=concepto
concepto=FechaEmisionCobro
FechaEmisionCobro=FechaEmisionRec
FechaEmisionRec=impuesto1
impuesto1=PrecioSinImp
PrecioSinImp=DescuentoImporteSinImp
DescuentoImporteSinImp=(Fin)

[Tabla.SQL]
(Inicio)=SELECT
SELECT=id,
id,=Cobro,
Cobro,=ConsecCobro,
ConsecCobro,=FechaEmisionCobro,
FechaEmisionCobro,=Cliente,
Cliente,=Nombre,
Nombre,=Importe,
Importe,=Impuestos,
Impuestos,=Recibo,
Recibo,=ConsecRec,
ConsecRec,=FechaEmisionRec,
FechaEmisionRec,=Articulo,
Articulo,=cantidad,
cantidad,=Precio,
Precio,=DescuentoLinea,
DescuentoLinea,=DescuentoImporte,
DescuentoImporte,=SolCargo,
SolCargo,=ConsecSolCargo,
ConsecSolCargo,=PreRegistro,
PreRegistro,=ConsecPre,
ConsecPre,=CDIclavepresupuestal,
CDIclavepresupuestal,=Descripcion,
Descripcion,=Curso,
Curso,=proyecto,
proyecto,=Ejercicio,
Ejercicio,=Periodo,
Periodo,=Subsidio,
Subsidio,=Orden,
Orden,=concepto,
concepto,=impuesto1,
impuesto1,=PrecioSinImp,
PrecioSinImp,=DescuentoImporteSinImp
DescuentoImporteSinImp=FROM CDICobrosInsc
FROM CDICobrosInsc=(Fin)

