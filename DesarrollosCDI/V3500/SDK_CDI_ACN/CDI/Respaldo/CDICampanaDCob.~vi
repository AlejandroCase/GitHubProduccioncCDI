[Vista.ListaCampos]
(Inicio)=CDICampanaDCob.Id
CDICampanaDCob.Id=CDICampanaDCob.Renglon
CDICampanaDCob.Renglon=CDICampanaDCob.IDMovCXC
CDICampanaDCob.IDMovCXC=CDICampanaDCob.MovCxC
CDICampanaDCob.MovCxC=CDICampanaDCob.MovIdCxC
CDICampanaDCob.MovIdCxC=CDICampanaDCob.FechaDocto
CDICampanaDCob.FechaDocto=CDICampanaDCob.Concepto
CDICampanaDCob.Concepto=CDICampanaDCob.Saldo
CDICampanaDCob.Saldo=CDICampanaDCob.Procesar
CDICampanaDCob.Procesar=CDICampanaDCob.IdCobro
CDICampanaDCob.IdCobro=CDICampanaDCob.IdCobroMov
CDICampanaDCob.IdCobroMov=CDICampanaDCob.IdCobroMovId
CDICampanaDCob.IdCobroMovId=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDICampanaDCob.Id,
CDICampanaDCob.Id,=CDICampanaDCob.Renglon,
CDICampanaDCob.Renglon,=CDICampanaDCob.IDMovCXC,
CDICampanaDCob.IDMovCXC,=CDICampanaDCob.MovCxC,
CDICampanaDCob.MovCxC,=CDICampanaDCob.MovIdCxC,
CDICampanaDCob.MovIdCxC,=CDICampanaDCob.FechaDocto,
CDICampanaDCob.FechaDocto,=CDICampanaDCob.Concepto,
CDICampanaDCob.Concepto,=CDICampanaDCob.Saldo,
CDICampanaDCob.Saldo,=CDICampanaDCob.Procesar,
CDICampanaDCob.Procesar,=CDICampanaDCob.IdCobro,
CDICampanaDCob.IdCobro,=CDICampanaDCob.IdCobroMov,
CDICampanaDCob.IdCobroMov,=CDICampanaDCob.IdCobroMovId
CDICampanaDCob.IdCobroMovId=FROM CDICampanaDCob
FROM CDICampanaDCob=(Fin)

[Vista.CamposBusquedaRapida]
(Inicio)=CDICampanaDCob.MovIdCxC
CDICampanaDCob.MovIdCxC=CDICampanaDCob.FechaDocto
CDICampanaDCob.FechaDocto=CDICampanaDCob.Concepto
CDICampanaDCob.Concepto=(Fin)
