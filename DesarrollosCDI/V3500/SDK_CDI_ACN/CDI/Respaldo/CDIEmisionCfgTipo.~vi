[Vista.SQL]
(Inicio)=SELECT
SELECT=CDIEmisionCfgTipo.Id,
CDIEmisionCfgTipo.Id,=CDIEmisionCfgTipo.Renglon,
CDIEmisionCfgTipo.Renglon,=CDIEmisionCfgTipo.Tipo
