[Vista.ListaCampos]
(Inicio)=CE.ID
CE.ID=CE.Empresa
CE.Empresa=CE.Mov
CE.Mov=CE.MovID
CE.MovID=CE.FechaEmision
CE.FechaEmision=CE.Concepto
CE.Concepto=CE.Proyecto
CE.Proyecto=CE.UEN
CE.UEN=CE.Moneda
CE.Moneda=CE.TipoCambio
CE.TipoCambio=CE.Usuario
CE.Usuario=CE.Referencia
CE.Referencia=CE.Observaciones
CE.Observaciones=CE.Estatus
CE.Estatus=CE.Situacion
CE.Situacion=CE.Programa
CE.Programa=CE.CEPlan
CE.CEPlan=CE.FechaInicio
CE.FechaInicio=CE.FechaFin
CE.FechaFin=CE.Condicion
CE.Condicion=CE.FechaCancelacion
CE.FechaCancelacion=CE.Sucursal
CE.Sucursal=CE.Importe
CE.Importe=CE.Impuestos
CE.Impuestos=CE.Cliente
CE.Cliente=CE.Cliente2
CE.Cliente2=CE.Porcentaje
CE.Porcentaje=CE.Porcentaje2
CE.Porcentaje2=CE.CDISocio
CE.CDISocio=Cte.Cliente
Cte.Cliente=Cte.Nombre
Cte.Nombre=(Fin)

[Vista.ListaTablas]
(Inicio)=CE
CE=Cte
Cte=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CE.ID,
CE.ID,=CE.Empresa,
CE.Empresa,=CE.Mov,
CE.Mov,=CE.MovID,
CE.MovID,=CE.FechaEmision,
CE.FechaEmision,=CE.Concepto,
CE.Concepto,=CE.Proyecto,
CE.Proyecto,=CE.UEN,
CE.UEN,=CE.Moneda,
CE.Moneda,=CE.TipoCambio,
CE.TipoCambio,=CE.Usuario,
CE.Usuario,=CE.Referencia,
CE.Referencia,=CE.Observaciones,
CE.Observaciones,=CE.Estatus,
CE.Estatus,=CE.Situacion,
CE.Situacion,=CE.Programa,
CE.Programa,=CE.CEPlan,
CE.CEPlan,=CE.FechaInicio,
CE.FechaInicio,=CE.FechaFin,
CE.FechaFin,=CE.Condicion,
CE.Condicion,=CE.FechaCancelacion,
CE.FechaCancelacion,=CE.Sucursal,
CE.Sucursal,=CE.Importe,
CE.Importe,=CE.Impuestos,
CE.Impuestos,=CE.Cliente,
CE.Cliente,=CE.Cliente2,
CE.Cliente2,=CE.Porcentaje,
CE.Porcentaje,=CE.Porcentaje2,
CE.Porcentaje2,=CE.CDISocio,
CE.CDISocio,=Cte.Cliente,
Cte.Cliente,=Cte.Nombre
Cte.Nombre=FROM CE
FROM CE=LEFT OUTER JOIN Cte ON CE.Cliente<IGUAL>Cte.Cliente
LEFT OUTER JOIN Cte ON CE.Cliente<IGUAL>Cte.Cliente=(Fin)

[Vista.CamposBusquedaRapida]
(Inicio)=CE.MovID
CE.MovID=Cte.Cliente
Cte.Cliente=CE.CDISocio
CE.CDISocio=(Fin)
