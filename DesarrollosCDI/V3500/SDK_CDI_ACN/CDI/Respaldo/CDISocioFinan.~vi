[Vista.ListaCampos]
(Inicio)=CDISocioFinan.Modulo
CDISocioFinan.Modulo=CDISocioFinan.Id
CDISocioFinan.Id=CDISocioFinan.mov
CDISocioFinan.mov=CDISocioFinan.movid
CDISocioFinan.movid=CDISocioFinan.FechaEmision
CDISocioFinan.FechaEmision=CDISocioFinan.Cliente
CDISocioFinan.Cliente=CDISocioFinan.CdiSocio
CDISocioFinan.CdiSocio=CDISocioFinan.importe
CDISocioFinan.importe=CDISocioFinan.Impuestos
CDISocioFinan.Impuestos=CDISocioFinan.Descuento
CDISocioFinan.Descuento=CDISocioFinan.DescuentoGlobal
CDISocioFinan.DescuentoGlobal=CDISocioFinan.Saldo
CDISocioFinan.Saldo=CDISocioFinan.EstatusDoc
CDISocioFinan.EstatusDoc=CDISocioFinan.Origen
CDISocioFinan.Origen=CDISocioFinan.Ejercicio
CDISocioFinan.Ejercicio=CDISocioFinan.Periodo
CDISocioFinan.Periodo=CDISocioFinan.Articulo
CDISocioFinan.Articulo=CDISocioFinan.cantidad
CDISocioFinan.cantidad=CDISocioFinan.precio
CDISocioFinan.precio=CDISocioFinan.Aplica
CDISocioFinan.Aplica=CDISocioFinan.AplicaId
CDISocioFinan.AplicaId=CDISocioFinan.descripcion1
CDISocioFinan.descripcion1=(Fin)

[Vista.SQL]
(Inicio)=SELECT
SELECT=CDISocioFinan.Modulo,
CDISocioFinan.Modulo,=CDISocioFinan.Id,
CDISocioFinan.Id,=CDISocioFinan.mov,
CDISocioFinan.mov,=CDISocioFinan.movid,
CDISocioFinan.movid,=CDISocioFinan.FechaEmision,
CDISocioFinan.FechaEmision,=CDISocioFinan.Cliente,
CDISocioFinan.Cliente,=CDISocioFinan.CdiSocio,
CDISocioFinan.CdiSocio,=CDISocioFinan.importe,
CDISocioFinan.importe,=CDISocioFinan.Impuestos,
CDISocioFinan.Impuestos,=CDISocioFinan.Descuento,
CDISocioFinan.Descuento,=CDISocioFinan.DescuentoGlobal,
CDISocioFinan.DescuentoGlobal,=CDISocioFinan.Saldo,
CDISocioFinan.Saldo,=CDISocioFinan.EstatusDoc,
CDISocioFinan.EstatusDoc,=CDISocioFinan.Origen,
CDISocioFinan.Origen,=CDISocioFinan.Ejercicio,
CDISocioFinan.Ejercicio,=CDISocioFinan.Periodo,
CDISocioFinan.Periodo,=CDISocioFinan.Articulo,
CDISocioFinan.Articulo,=CDISocioFinan.cantidad,
CDISocioFinan.cantidad,=CDISocioFinan.precio,
CDISocioFinan.precio,=CDISocioFinan.Aplica,
CDISocioFinan.Aplica,=CDISocioFinan.AplicaId,
CDISocioFinan.AplicaId,=CDISocioFinan.descripcion1
CDISocioFinan.descripcion1=FROM CDISocioFinan
FROM CDISocioFinan=(Fin)

[Vista.CamposBusquedaRapida]
(Inicio)=CDISocioFinan.Cliente
CDISocioFinan.Cliente=CDISocioFinan.CdiSocio
CDISocioFinan.CdiSocio=CDISocioFinan.Articulo
CDISocioFinan.Articulo=CDISocioFinan.mov
CDISocioFinan.mov=CDISocioFinan.movid
CDISocioFinan.movid=CDISocioFinan.precio
CDISocioFinan.precio=CDISocioFinan.Aplica
CDISocioFinan.Aplica=CDISocioFinan.AplicaId
CDISocioFinan.AplicaId=(Fin)
